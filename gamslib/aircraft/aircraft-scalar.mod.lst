  63   ed(j)      = sum(h, lambda(j,h)*dd(j,h));
  64   gamma(j,h) = sum(hp$(ord(hp) ge ord(h)), lambda(j,hp));
  65   deltb(j,h) = (dd(j,h)-dd(j,h-1))$dd(j,h);
