  20  variables
  21     ham   'expected value of hamiltonian'
  22     alpha 'variational parameter'
  23     n     'variational parameter (n=1: Gaussian trial function)';
  24   
  25  equation
  26     hamiltonian;
  27   
  28  scalar g /1/;
  29   
  30  hamiltonian..
  31   
  32     ham =e= (sqr(n)/2)*(gamma(2-1/(2*n))/gamma(1/(2*n)))*(alpha**(1/n))
