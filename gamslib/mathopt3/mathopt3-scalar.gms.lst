  18  variables x1, x2, x3, x4, x5, x6, obj;
  19  equations defobj,eq1,eq2,eq3,eq4,ineq1,ineq2,ineq3;
  20   
  21  defobj.. obj =e= sqr(x1 + x2) + sqr(x3 - x5) + sqr(x6 - x4)
  22                 + 2*sqr(x1 + x3 - x4) + sqr(x2 - x1 + x3 - x4)
  23                 + 10*sqr(Sin[x5 - x6 + x1]);
  24   
  25  eq1..  sqr(x1) - Sin[x2] - x4 + x5 + x6 =e= 0;
  26  eq2..  x1*x3 - x2*x4*x1 - x5 - Sin[x6 - x1 - x3] =e= 0;
  27  eq3..  x2*x6*Cos[x5] - Sin[x3*x4] + x2 - x5 =e= 0;
  28  eq4..  x1*x2 -sqr(x3) - x4*x5 - sqr(x6) =e= 0;
  29   
  30  ineq1.. 2*x1 + 5*x2 + x3 + x4  -1 =l= 0;
  31  ineq2.. 3*x1 - 2*x2 + x3 - 4*x4 =l= 0;
  32  ineq3.. x1 + x2 + x3 + x4 + x5 + x6 - 2 =l= 0;
  33   
  34  model m / all /;
  35   
  36  * most local solvers will find the global solution from this starting point
  37  * x1.l= 1; x2.l= -2; x3.l= 1; x4.l= 2; x5.l= 1; x6.l= -1;
  38  * solve m us nlp min obj;
  39   
  40  x1.l=10; x2.l=-10; x3.l=10; x4.l=10; x5.l=10; x6.l=-10;
  41   
  42  solve m us nlp min obj;
  43   
  44  parameter report diff from global solution;
  45   
  46  report('x1') = round(0 - x1.l,6);
