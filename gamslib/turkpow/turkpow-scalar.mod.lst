 214   report("power",b,t) = d(b,t);                           report("duration",b,t)  = dur(b);
 215   report("power","power",t) = sum(b, report("power",b,t));
 216   report(m,"power",t)       = sum(b, report(m,b,t));
 217   report(mt,"capacity",t)   = sum(v$vs(t,v), kit(mt,v) + ht.l(mt,v));
 218   report(mh,"capacity",t)   = mdatah(mh,"initcap") + sum(v$vs(t,v), hh.l(mh,v));
 219   
 220   report("energy",b,t)        = report("power",b,t)*report("duration",b,t);
 221   report("energy","energy",t) = sum(b, report("energy",b,t));
 222   report(m,"energy",t)        = sum(b, dur(b)*report(m,b,t));
 223   report(mh,"e-limit",t)      = report(mh,"capacity",t)*mdatah(mh,"e-fact");
 224   
 225   report(mh,"op-cost",t)   = report(mh,"energy",t)*mdatah(mh,"opcost");
 226   report(mt,"op-cost",t)   = sum(v$vs(t,v), opcostt(mt,v,t)*sum(b, dur(b)*zt.l(mt,v,b,t)));
 227   report("op-cost","op-cost",t) = sum(m, report(m,"op-cost",t));
 228   report("op-cost",b,t) = sum(mh, dur(b)*mdatah(mh,"opcost")*zh.l(mh,b,t))
 229                      + sum((mt,v)$vs(t,v), dur(b)*opcostt(mt,v,t)*zt.l(mt,v,b,t));
 230   
