 133   length(time) = ord(time) - 1;
 134   
 135   bs(b,bp)  = 1$(ord(b) ge ord(bp));
 136   vs(t,v)  = 1$(ord(t) ge ord(v));
 137   opcostt(m,v,t)$vs(t,v)    = mdatat(m,"opcost")*(1 + mdatat(m,"opcost-g"))**length(v);
 138   capcostt(m,v,t)$vs(t,v)   = mdatat(m,"capcost")*(1 + mdatat(m,"capcost-g"))**length(v);
 139   
 140   d(b,te) = round(dd(b,"demand")*(1 + g)**length(te),0);
 141   dur(b)  = sum(bp$bs(b,bp), dd(bp,"duration")) / sum(bp, dd(bp,"duration"));
 142   delta(t) = (1+rho)**(-length(t))  ;
 143   sigma(mt) = rho/(1-(1+rho)**(-mdatat(mt,"life")))  ;
 144   sigma(mh) = rho/(1-(1+rho)**(-mdatah(mh,"life")))  ;
