 141  * converting variables into original form
 142  actv(j)  = exp(v.l(j)) ;
 143  actb(i)  = exp(b.l(i)) ;
 144  acttl(i) = exp(tl.l(i));
 145  actn(j)  = exp(n.l(j)) ;
 146   
 147  * optimal design
 148  Display actv, actb, acttl, actn ;


