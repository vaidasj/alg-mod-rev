  60   
  61  equations
  62     c1(i,j)   for each cell pick only one item pair
  63     c2(k,l)   an item pair can show up only once
  64     c3(i,l)   items have to be unique in each column for square one
  65     c4(j,l)   items have to be unique in each column for square two
  66     c5(i,k)   items have to be unique in each row for square one
  67     c6(j,k)   items have to be unique in each row for square two
  68     obj       some objective function;
  69   
  70  c1(i,j)..     sum((k,l), x(i,j,k,l)) =e= 1;
  71  c2(k,l)..     sum((i,j), x(i,j,k,l)) =e= 1;
  72  c3(i,l)..     sum((j,k), x(i,j,k,l)) =e= 1;
  73  c4(j,l)..     sum((i,k), x(i,j,k,l)) =e= 1;
  74  c5(i,k)..     sum((j,l), x(i,j,k,l)) =e= 1;
  75  c6(j,k)..     sum((i,l), x(i,j,k,l)) =e= 1;
  76  obj..   z =e= sum((i,j,k,l), x(i,j,k,l));
  77   
  78  model latin / all /;
  79   
  80  parameter report(*,k,l); option report:0:1:1;
  81   
       
      * force an incorrect solution
      x.fx('val-1','val-1','row-1','col-1') = 1;
       
      solve latin minimizing z using mip;
       
       
      loop((i,j,k,l)$x.l(i,j,k,l),
         report('one',k,l) = ord(i);
         report('two',k,l) = ord(j); );
       
      display report;
       
