  60  INTEGER VARIABLES np, ns;
  61  BINARY VARIABLE z;
  62   
  63  EQUATIONS
  64           f         Objective function
  65           g(i)      Power output calculation for level i
  66           gd(i)     Pressure rise calculation for level i
  67           sumx      Constraint on volume fractions
  68           gvdot(i)  Volume flowrate calculation for pumps on level i
  69           gdp(i)    Constraints on pressure rise
  70           lw(i)     Logical constraints on w
  71           lP(i)     Logical constraints on P
  72           ldp(i)    Logical constraints dp
  73           lvdot(i)  Logical constraints on vdot
  74           lx(i)     Logical constraints on x
  75           lnp(i)    Logical constraints on np
  76           lns(i)    Logical constraints on ns;
  77   
  78  f.. objval =e= SUM(i, (C(i) + Cd(i)*P(i)) * np(i)*ns(i)*z(i));
  79   
  80  g(i).. P(i) =e= ldata(i,'m1') * POWER(w(i)/wmax,3) +
  81                  ldata(i,'m2') * POWER(w(i)/wmax,2)*vdot(i) -
  82                  ldata(i,'m3') * w(i)/wmax*POWER(vdot(i),2);
  83   
  84  gd(i).. dp(i) =e=  ldata(i,'m4') * w(i)/wmax*vdot(i) +
  85                     ldata(i,'m5') * POWER(w(i)/wmax,2)  -
  86                     ldata(i,'m6') * POWER(vdot(i),2);
  87   
  88  sumx.. SUM(i,x(i)) =e= 1;
  89   
  90  gvdot(i).. x(i) =e= vdot(i)/Vtot * np(i);
  91   
  92  gdp(i)..   z(i) =e= dp(i) / dPtot * ns(i);
  93   
  94  lw(i)..    w(i) / wmax =l= z(i);
  95  lP(i)..    P(i) / Pmax(i) =l= z(i);
  96  ldp(i)..   dp(i) / dPtot =l= z(i);
  97  lvdot(i).. vdot(i) / Vtot =l= z(i);
  98  lx(i)..    x(i) =l= z(i);
  99  lnp(i)..   np(i) =l= npmax * z(i);
 100  lns(i)..   ns(i) =l= nsmax * z(i);
