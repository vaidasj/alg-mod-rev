  50  b.l = (maxteeth-minteeth)/2;
  51  d.l = (maxteeth-minteeth)/2;
  52   
  53  model m1 /all/;
  54   
  55  solve m1 using minlp minimizing z;
  56   
  57  Scalar  calcinvratio ratio of solution
