  65  * setect subset of stocks and periods
  66  d(days)   = ord(days) >1 and ord(days) < 31;
  67  s(stocks) = ord(stocks) < 51;
  68   
  69  parameter mean(stocks)          mean of daily return
  70            dev(stocks,days)      deviations
  71            covar(stocks,sstocks) covariance matrix of returns (upper)
  72            totmean               total mean return;
  73   
  74  mean(s)  = sum(d, return(s,d))/card(d);
  75  dev(s,d) = return(s,d)-mean(s);
  76   
  77  * calculate covariance
  78  * to save memory and time we only compute the uppertriangular
  79  * part as the covariance matrix is symmetric
  80  covar(upper(s,t)) = sum(d, dev(s,d)*dev(t,d))/(card(d)-1);
  81   
  82  totmean = sum(s, mean(s))/(card(s));
  83   
  84  variables z          objective variable
  85            x(stocks)  investments;
  86   
  87  positive variables x;
  88   
  89  equations obj    objective
  90            budget
  91            retcon return constraint
  92            ;
  93   
  94   
  95  obj.. z =e= sum(upper(s,t), x(s)*covar(s,t)*x(t)) +
  96              sum(lower(s,t), x(s)*covar(t,s)*x(t));
  97  budget.. sum(s, x(s)) =e= 1.0;
  98  retcon.. sum(s, mean(s)*x(s)) =g= totmean*1.25;
  99   
 100  model qp1 /all/;
 101   
 102  * Some solvers need more memory
 103  qp1.workfactor = 10;
 104  solve qp1 using nlp minimizing z;
 105  display x.l;
