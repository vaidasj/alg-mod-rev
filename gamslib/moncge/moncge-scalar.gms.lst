 227  ;
 228  *[domestic production] ----
 229  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 230  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 231  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 232  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 233  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 234   
 235  *[government behavior] ----
 236  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 237  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 238  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 239  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 240                              -Sg)/pq(i);
 241   
 242  *[investment behavior] ----
 243  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 244   
 245  *[savings] ----------------
 246  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 247  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 248   
 249  *[household consumption] --
 250  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td
 251                              +sum(j, RT(j)))/pq(i);
 252  eqRT(j)..       RT(j)   =e= (1-eta(j))/eta(j)*pd(j)*D(j);
 253   
 254  *[international trade] ----
 255  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 256  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 257  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 258                          =e= sum(i, pWm(i)*M(i));
 259   
 260  *[Armington function] -----
 261  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)
 262                              +deltad(i)*D(i)**eta(i))**(1/eta(i));
 263  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 264                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 265  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)
 266                              /((1/eta(i))*pd(i)))**(1/(1-eta(i)))*Q(i);
 267   
 268  *[transformation function] -----
 269  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 270                              *D(i)**phi(i))**(1/phi(i));
