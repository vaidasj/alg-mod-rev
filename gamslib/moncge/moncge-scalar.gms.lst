 112  * Calibration ---------------------------------------------------------
 113   
 114  Parameter       alpha(i)        share parameter in utility func.
 115                  beta(h,j)       share parameter in production func.
 116                  b(j)            scale parameter in production func.
 117                  ax(i,j)         intermediate input requirement coeff.
 118                  ay(j)           composite fact. input req. coeff.
 119                  mu(i)           government consumption share
 120                  lambda(i)       investment demand share
 121                  deltam(i)       share parameter in Armington func.
 122                  deltad(i)       share parameter in Armington func.
 123                  gamma(i)        scale parameter in Armington func.
 124                  xid(i)          share parameter in transformation func.
 125                  xie(i)          share parameter in transformation func.
 126                  theta(i)        scale parameter in transformation func.
 127                  ssp             average propensity for private saving
 128                  ssg             average propensity for government saving
 129                  taud            direct tax rate
 130  ;
 131   
 132  alpha(i)=Xp0(i)/sum(j, Xp0(j));
 133  beta(h,j)=F0(h,j)/sum(k, F0(k,j));
 134  b(j)    =Y0(j)/prod(h, F0(h,j)**beta(h,j));
 135   
 136  ax(i,j) =X0(i,j)/Z0(j);
 137  ay(j)   =Y0(j)/Z0(j);
 138  mu(i)   =Xg0(i)/sum(j, Xg0(j));
 139  lambda(i)=Xv0(i)/(Sp0+Sg0+Sf);
 140   
 141  deltam(i)=(1+taum(i))*M0(i)**(1-eta(i))
 142          /((1+taum(i))*M0(i)**(1-eta(i))
 143          +(1/eta(i))*D0(i)**(1-eta(i)));
 144  deltad(i)=(1/eta(i))*D0(i)**(1-eta(i))
 145          /((1+taum(i))*M0(i)**(1-eta(i))
 146          +(1/eta(i))*D0(i)**(1-eta(i)));
 147  gamma(i)=Q0(i)/(deltam(i)*M0(i)**eta(i)+deltad(i)*D0(i)**eta(i))
 148          **(1/eta(i));
 149   
 150  xie(i)=E0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 151  xid(i)=D0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 152  theta(i)=Z0(i)/(xie(i)*E0(i)**phi(i)+xid(i)*D0(i)**phi(i))**(1/phi(i));
 153   
 154  ssp     =Sp0/(sum(h, FF(h))+sum(j, RT0(j)));
 155  ssg     =Sg0/(Td0+sum(j, Tz0(j))+sum(j, Tm0(j)));
 156  taud    =Td0/(sum(h, FF(h))+sum(j, RT0(j)));
