 226  ;
 227  *[domestic production] ----
 228  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 229  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 230  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 231  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 232  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 233   
 234  *[government behavior] ----
 235  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 236  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 237  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 238  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 239                              -Sg)/pq(i);
 240   
 241  *[investment behavior] ----
 242  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 243   
 244  *[savings] ----------------
 245  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 246  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 247   
 248  *[household consumption] --
 249  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td
 250                              +sum(j, RT(j)))/pq(i);
 251  eqRT(j)..       RT(j)   =e= (1-eta(j))/eta(j)*pd(j)*D(j);
 252   
 253  *[international trade] ----
 254  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 255  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 256  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 257                          =e= sum(i, pWm(i)*M(i));
 258   
 259  *[Armington function] -----
 260  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)
 261                              +deltad(i)*D(i)**eta(i))**(1/eta(i));
 262  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 263                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 264  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)
 265                              /((1/eta(i))*pd(i)))**(1/(1-eta(i)))*Q(i);
 266   
 267  *[transformation function] -----
 268  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 269                              *D(i)**phi(i))**(1/phi(i));
