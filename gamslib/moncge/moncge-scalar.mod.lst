 111  * Calibration ---------------------------------------------------------
 112   
 113  Parameter       alpha(i)        share parameter in utility func.
 114                  beta(h,j)       share parameter in production func.
 115                  b(j)            scale parameter in production func.
 116                  ax(i,j)         intermediate input requirement coeff.
 117                  ay(j)           composite fact. input req. coeff.
 118                  mu(i)           government consumption share
 119                  lambda(i)       investment demand share
 120                  deltam(i)       share parameter in Armington func.
 121                  deltad(i)       share parameter in Armington func.
 122                  gamma(i)        scale parameter in Armington func.
 123                  xid(i)          share parameter in transformation func.
 124                  xie(i)          share parameter in transformation func.
 125                  theta(i)        scale parameter in transformation func.
 126                  ssp             average propensity for private saving
 127                  ssg             average propensity for government saving
 128                  taud            direct tax rate
 129  ;
 130   
 131  alpha(i)=Xp0(i)/sum(j, Xp0(j));
 132  beta(h,j)=F0(h,j)/sum(k, F0(k,j));
 133  b(j)    =Y0(j)/prod(h, F0(h,j)**beta(h,j));
 134   
 135  ax(i,j) =X0(i,j)/Z0(j);
 136  ay(j)   =Y0(j)/Z0(j);
 137  mu(i)   =Xg0(i)/sum(j, Xg0(j));
 138  lambda(i)=Xv0(i)/(Sp0+Sg0+Sf);
 139   
 140  deltam(i)=(1+taum(i))*M0(i)**(1-eta(i))
 141          /((1+taum(i))*M0(i)**(1-eta(i))
 142          +(1/eta(i))*D0(i)**(1-eta(i)));
 143  deltad(i)=(1/eta(i))*D0(i)**(1-eta(i))
 144          /((1+taum(i))*M0(i)**(1-eta(i))
 145          +(1/eta(i))*D0(i)**(1-eta(i)));
 146  gamma(i)=Q0(i)/(deltam(i)*M0(i)**eta(i)+deltad(i)*D0(i)**eta(i))
 147          **(1/eta(i));
 148   
 149  xie(i)=E0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 150  xid(i)=D0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 151  theta(i)=Z0(i)/(xie(i)*E0(i)**phi(i)+xid(i)*D0(i)**phi(i))**(1/phi(i));
 152   
 153  ssp     =Sp0/(sum(h, FF(h))+sum(j, RT0(j)));
 154  ssg     =Sg0/(Td0+sum(j, Tz0(j))+sum(j, Tm0(j)));
 155  taud    =Td0/(sum(h, FF(h))+sum(j, RT0(j)));
