 113  * Calibration ---------------------------------------------------------
 114   
 115  Parameter       alpha(i)        share parameter in utility func.
 116                  beta(h,j)       share parameter in production func.
 117                  b(j)            scale parameter in production func.
 118                  ax(i,j)         intermediate input requirement coeff.
 119                  ay(j)           composite fact. input req. coeff.
 120                  mu(i)           government consumption share
 121                  lambda(i)       investment demand share
 122                  deltam(i)       share parameter in Armington func.
 123                  deltad(i)       share parameter in Armington func.
 124                  gamma(i)        scale parameter in Armington func.
 125                  xid(i)          share parameter in transformation func.
 126                  xie(i)          share parameter in transformation func.
 127                  theta(i)        scale parameter in transformation func.
 128                  ssp             average propensity for private saving
 129                  ssg             average propensity for government saving
 130                  taud            direct tax rate
 131  ;
 132   
 133  alpha(i)=Xp0(i)/sum(j, Xp0(j));
 134  beta(h,j)=F0(h,j)/sum(k, F0(k,j));
 135  b(j)    =Y0(j)/prod(h, F0(h,j)**beta(h,j));
 136   
 137  ax(i,j) =X0(i,j)/Z0(j);
 138  ay(j)   =Y0(j)/Z0(j);
 139  mu(i)   =Xg0(i)/sum(j, Xg0(j));
 140  lambda(i)=Xv0(i)/(Sp0+Sg0+Sf);
 141   
 142  deltam(i)=(1+taum(i))*M0(i)**(1-eta(i))
 143          /((1+taum(i))*M0(i)**(1-eta(i))
 144          +(1/eta(i))*D0(i)**(1-eta(i)));
 145  deltad(i)=(1/eta(i))*D0(i)**(1-eta(i))
 146          /((1+taum(i))*M0(i)**(1-eta(i))
 147          +(1/eta(i))*D0(i)**(1-eta(i)));
 148  gamma(i)=Q0(i)/(deltam(i)*M0(i)**eta(i)+deltad(i)*D0(i)**eta(i))
 149          **(1/eta(i));
 150   
 151  xie(i)=E0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 152  xid(i)=D0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 153  theta(i)=Z0(i)/(xie(i)*E0(i)**phi(i)+xid(i)*D0(i)**phi(i))**(1/phi(i));
 154   
 155  ssp     =Sp0/(sum(h, FF(h))+sum(j, RT0(j)));
 156  ssg     =Sg0/(Td0+sum(j, Tz0(j))+sum(j, Tm0(j)));
 157  taud    =Td0/(sum(h, FF(h))+sum(j, RT0(j)));
