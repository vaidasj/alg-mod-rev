 228  ;
 229  *[domestic production] ----
 230  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 231  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 232  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 233  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 234  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 235   
 236  *[government behavior] ----
 237  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 238  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 239  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 240  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 241                              -Sg)/pq(i);
 242   
 243  *[investment behavior] ----
 244  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 245   
 246  *[savings] ----------------
 247  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h))+sum(j, RT(j)));
 248  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 249   
 250  *[household consumption] --
 251  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td
 252                              +sum(j, RT(j)))/pq(i);
 253  eqRT(j)..       RT(j)   =e= (1-eta(j))/eta(j)*pd(j)*D(j);
 254   
 255  *[international trade] ----
 256  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 257  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 258  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 259                          =e= sum(i, pWm(i)*M(i));
 260   
 261  *[Armington function] -----
 262  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)
 263                              +deltad(i)*D(i)**eta(i))**(1/eta(i));
 264  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 265                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 266  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)
 267                              /((1/eta(i))*pd(i)))**(1/(1-eta(i)))*Q(i);
 268   
 269  *[transformation function] -----
 270  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 271                              *D(i)**phi(i))**(1/phi(i));
