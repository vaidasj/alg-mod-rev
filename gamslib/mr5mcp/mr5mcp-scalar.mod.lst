 143          loop(t,   udf(rg,t+1)  = udf(rg,t) /(1+udr(rg,t))**ny; );
 144          udf(rg,tlast) = udf(rg,tlast) / (1-1/(1+udr(rg,tlast))**ny);
 145          udf(rg,t) = udf(rg,t) / sum(pp, udf(rg,pp));
 146          display udr, udf;
