 120  *       convert growth rates from percentages to fractions:
 121   
 122          grow(t,rg) = grow(t,rg) / 100;
 123          ksrv(rg) = 1/(1+depr(rg))**ny;
 124          l0(rg) = y0(rg) - kpvs(rg) * y0(rg);
 125          qref(tbase,rg) = 1;
 126          loop(t, qref(t+1,rg) = qref(t,rg) * (1 + grow(t,rg))**ny;);
 127          rk0(rg) = kpvs(rg) / kgdp(rg);
 128          udr(rg,t) = rk0(rg) - depr(rg) - grow(t,rg);
 129          pref(tbase,rg) = 1;
 130          pref(tfirst,rg) = 1;
 131          loop(t$(ord(t) gt 1),
 132            pref(t+1,rg) = pref(t,rg) /((1+udr(rg,t))*(1+grow(t,rg)))**ny );
 133          display pref, qref;
 134   
 135          l(rg,pp)   = l0(rg) * qref(pp,rg);
 136          loop(tbase, i0(rg) = k0(rg)*(grow(tbase,rg)+depr(rg)); );
 137          c0(rg) = y0(rg) - i0(rg);
 138          wb(rg) = c0(rg)*sum(pp, pref(pp,"usa")*qref(pp,rg));
 139   
