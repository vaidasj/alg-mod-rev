 122  *       convert growth rates from percentages to fractions:
 123   
 124          grow(t,rg) = grow(t,rg) / 100;
 125          ksrv(rg) = 1/(1+depr(rg))**ny;
 126          l0(rg) = y0(rg) - kpvs(rg) * y0(rg);
 127          qref(tbase,rg) = 1;
 128          loop(t, qref(t+1,rg) = qref(t,rg) * (1 + grow(t,rg))**ny;);
 129          rk0(rg) = kpvs(rg) / kgdp(rg);
 130          udr(rg,t) = rk0(rg) - depr(rg) - grow(t,rg);
 131          pref(tbase,rg) = 1;
 132          pref(tfirst,rg) = 1;
 133          loop(t$(ord(t) gt 1),
 134            pref(t+1,rg) = pref(t,rg) /((1+udr(rg,t))*(1+grow(t,rg)))**ny );
 135          display pref, qref;
 136   
 137          l(rg,pp)   = l0(rg) * qref(pp,rg);
 138          loop(tbase, i0(rg) = k0(rg)*(grow(tbase,rg)+depr(rg)); );
 139          c0(rg) = y0(rg) - i0(rg);
 140          wb(rg) = c0(rg)*sum(pp, pref(pp,"usa")*qref(pp,rg));
 141   
 142  *       compute the utility discount factor:
 143   
 144          udf(rg,tbase) = 1;
 145          loop(t,   udf(rg,t+1)  = udf(rg,t) /(1+udr(rg,t))**ny; );
 146          udf(rg,tlast) = udf(rg,tlast) / (1-1/(1+udr(rg,tlast))**ny);
 147          udf(rg,t) = udf(rg,t) / sum(pp, udf(rg,pp));
 148          display udr, udf;
