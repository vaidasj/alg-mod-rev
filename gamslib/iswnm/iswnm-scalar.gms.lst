 454       Parameter step  grid point for function
 455                 ftn   function value
 456                 ftna  interpolated function output
 457                 ftni  function input
 458                 ftno  function output
 459                 ;
 460   ftni(g) = function("input",g);  ftno(g) = function("output",g);
 461   step(n1,n,g) = (ftni(g) le lw(n1,n,"width")) and
 462                  (ftni(g+1) gt lw(n1,n,"width") );
 463   ftna(n1,n,g) = (ftno(g) +(lw(n1,n,"width")-ftni(g))*(ftno(g+1)-
 464                   ftno(g))/(ftni(g+1)-ftni(g)))$step(n1,n,g);
 465   ftn(n1,n) = sum(g, ftna(n1,n,g))  ;
 466   
 467   philc(n1,n)  = lw(n1,n,"length")*5000*ftn(n1,n)*per*caf;
