 471               *cmft*linkpar("pex",n1,n,m) + philc(n1,n)*madjust(n1,n,m)*(
 472                 linkpar("hcx",n1,n,m)-linkpar("hwr",n1,n,m)))/
 473                 ncap(n1,n)/1000000;
