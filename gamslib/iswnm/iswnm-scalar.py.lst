 454   
 455       Parameter step  grid point for function
 456                 ftn   function value
 457                 ftna  interpolated function output
 458                 ftni  function input
 459                 ftno  function output
 460                 ;
 461   ftni(g) = function("input",g);  ftno(g) = function("output",g);
 462   step(n1,n,g) = (ftni(g) le lw(n1,n,"width")) and
 463                  (ftni(g+1) gt lw(n1,n,"width") );
 464   ftna(n1,n,g) = (ftno(g) +(lw(n1,n,"width")-ftni(g))*(ftno(g+1)-
 465                   ftno(g))/(ftni(g+1)-ftni(g)))$step(n1,n,g);
 466   ftn(n1,n) = sum(g, ftna(n1,n,g))  ;
 467   
 468   philc(n1,n)  = lw(n1,n,"length")*5000*ftn(n1,n)*per*caf;
 469   
