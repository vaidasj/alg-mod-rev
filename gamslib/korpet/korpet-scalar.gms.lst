 435   
 436   mbr(cr,i,t)..  sum(p, a(cr,cr,p)*z(cr,p,i,t)) + vr(cr,i,t) =g= 0;
 437   
 438   mbir(cr,cb,i,t)..  sum(p, a(cr,cb,p)*z(cr,p,i,t)) + sum(ip$tc(i,ip), xi(cr,cb,ip,i,t) - xi(cr,cb,i,ip,t))$cs(cb)
 439                  =g= sum(cfr$cfcb(cfr,cb), w(cr,cb,cfr,i,t));
 440   
 441   mbip(cr,ci,i,t)..
 442     sum(p, a(cr,ci,p)*z(cr,p,i,t)) + sum(ip$tc(i,ip), xi(cr,ci,ip,i,t) - xi(cr,ci,i,ip,t))$cs(ci)  =g= 0;
 443   
 444   mbfr(cfr,i,t)..  sum((cr,cb)$cfcb(cfr,cb), w(cr,cb,cfr,i,t)) =e= sum(j, xf(cfr,i,j,t));
 445   
 446   mbfp(cfp,i,t)..  sum((cr,p), a(cr,cfp,p)*z(cr,p,i,t)) =g= sum(j, xf(cfp,i,j,t)) + e(cfp,i,t);
 447   
 448   qcfl(cfr,q,i,t)$qll(cfr,q)..
 449     sum((cr,cb)$cfcb(cfr,cb), at(cr,cb,q)*w(cr,cb,cfr,i,t)) =g= sum(j, qll(cfr,q)*xf(cfr,i,j,t));
