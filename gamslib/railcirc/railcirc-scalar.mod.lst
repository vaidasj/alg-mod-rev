 156  Alias (t,tt), (s,ss);
 157   
 158  * Construct the timetable graph
 159   
 160  loop((z,s,t,ss,tt)$sum(c,timetable(z,s,t,ss,tt,c)),
 161    ste(s,t) = yes; ste(ss,tt) = yes;  // station timetable events
 162    is(s,t,ss,tt) = yes;               // in-service arcs
