 241  f.l("tu2",g) = 0;
 242  nscirc.tryint = 0.1;
 243   
 244  solve nscirc using mip minimizing obj; // this is a real MIP
 245   
 246  rep('tu1+tu2 ',tup) = sum(on, f.l(tup,on));
 247  rep('tu1+tu2 ','maxcars') = maxcars;
 248  rep('tu1+tu2 ','Total cost') = obj.l;
