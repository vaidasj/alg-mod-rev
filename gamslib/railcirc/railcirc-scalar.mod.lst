 166  loop(s,
 167  * The first station time event of the day
 168    first_ste(ss,tt) = no;
 169    loop(ste(s,t)$(not card(first_ste)), first_ste(ste) = yes);
 170   
 171  * All adjacent station time events are the in-station arcs
 172    last_ste(first_ste)  = yes;
 173    loop(ste(s,t)$(not first_ste(ste)),
 174      g(last_ste(s,tt),ste) = yes;
 175      last_ste(ss,tt) = no;
 176      last_ste(ste)   = yes;
 177    );
 178   
 179  * Don't forget the overnight arc
 180    on(last_ste,first_ste) = yes;
 181    g(on) = yes;
 182  );
 183   
 184   
 185  Variable  f(tu,s,t,s,t)         the flow of train units
 186            obj                   the objective variable
 187  Integer Variable f;
 188   
 189  Equations circulation(tu,s,t)   inflow is equal outflow at each node
 190            demand(s,t,s,t,c)     demand of first and second class seats
 191            defmaxcars(s,t,s,t)   maximum cars on in-service arcs
 192            defobj                objective function;
 193   
 194  circulation(tup,ste(s,t)).. sum(g(ss,tt,ste),f(tup,g)) =e=
 195                              sum(g(ste,ss,tt),f(tup,g));
 196   
 197  demand(is,c)..     sum(z,timetable(z,is,c)) =l=
 198                     sum(tup, f(tup,is)*trainunitdata(tup,c));
 199   
 200  defmaxcars(is)..   maxcars =g=
 201                     sum(tup, f(tup,is)*trainunitdata(tup,'NumberCars'));
 202   
 203  defobj..           obj =e= sum((tup,on), f(tup,on)*trainunitdata(tup,'Cost'));
 204   
 205  Model nscirc /all/ ;
 206  option optcr=0;
 207   
 208  * If we do one type of trainunit at a time, we can tighten the demand equation
 209  f.lo(tu,is) = smax(c,ceil(sum(z,timetable(z,is,c)/trainunitdata(tu,c))));
 210   
 211  Parameter rep solution report;
 212   
 213  * Now trainunit tu1
 214  tup('tu1') = yes; tup('tu2') = no;
 215  solve nscirc using mip minimizing obj;
 216   
 217  rep('tu1 only',tup) = sum(on, f.l(tup,on));
 218  rep('tu1 only','Total cost') = obj.l;
