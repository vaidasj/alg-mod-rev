 150  Set g(s,t,s,t) timetable graph
 151     is(s,t,s,t) in-service arcs
 152     on(s,t,s,t) overnight arcs
 153        ste(s,t) station timetable events
 154  first_ste(s,t) first station timetable event of the day
 155   last_ste(s,t) last station timetable event of the day
 156         tup(tu) subset of train units in the model
 157   
 158  Alias (t,tt), (s,ss);
 159   
 160  * Construct the timetable graph
 161   
 162  loop((z,s,t,ss,tt)$sum(c,timetable(z,s,t,ss,tt,c)),
 163    ste(s,t) = yes; ste(ss,tt) = yes;  // station timetable events
 164    is(s,t,ss,tt) = yes;               // in-service arcs
 165    g(s,t,ss,tt) = yes;
 166  );
 167   
