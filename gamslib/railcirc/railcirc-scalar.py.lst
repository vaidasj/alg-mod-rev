 221  rep('tu1 only','maxcars') = maxcars;
 222   
 223  * Now trainunit tu2
 224  tup('tu1') = no; tup('tu2') = yes;
 225   
 226  * We have to reset maxcars to 16 because of service Rtd.1701 to Rsd.1743
 227  * which requires 4 trains units of type tu2 and therefore 16 cars.
 228  maxcars = 16;
 229  solve nscirc using mip minimizing obj;
 230   
 231  rep('tu2 only',tup) = sum(on, f.l(tup,on));
 232  rep('tu2 only','maxcars') = maxcars;
 233  rep('tu2 only','Total cost') = obj.l;
 234   
 235  * Now both trainunits
 236  tup('tu1') = yes; tup('tu2') = yes;
 237   
 238  * Undo the tightening and go back to the original maxcars
 239  maxcars = 15;
 240  f.lo(tu,is) = 0;
 241   
 242  * Take the 'tu1 only' single trainunit solution as a start
 243  f.l("tu2",g) = 0;
 244  nscirc.tryint = 0.1;
 245   
 246  solve nscirc using mip minimizing obj; // this is a real MIP
 247   
 248  rep('tu1+tu2 ',tup) = sum(on, f.l(tup,on));
 249  rep('tu1+tu2 ','maxcars') = maxcars;
 250  rep('tu1+tu2 ','Total cost') = obj.l;
 251   
 252  display rep;


