 168  loop(s,
 169  * The first station time event of the day
 170    first_ste(ss,tt) = no;
 171    loop(ste(s,t)$(not card(first_ste)), first_ste(ste) = yes);
 172   
 173  * All adjacent station time events are the in-station arcs
 174    last_ste(first_ste)  = yes;
 175    loop(ste(s,t)$(not first_ste(ste)),
 176      g(last_ste(s,tt),ste) = yes;
 177      last_ste(ss,tt) = no;
 178      last_ste(ste)   = yes;
 179    );
 180   
 181  * Don't forget the overnight arc
 182    on(last_ste,first_ste) = yes;
 183    g(on) = yes;
 184  );
 185   
 186   
 187  Variable  f(tu,s,t,s,t)         the flow of train units
 188            obj                   the objective variable
 189  Integer Variable f;
 190   
 191  Equations circulation(tu,s,t)   inflow is equal outflow at each node
 192            demand(s,t,s,t,c)     demand of first and second class seats
 193            defmaxcars(s,t,s,t)   maximum cars on in-service arcs
 194            defobj                objective function;
 195   
 196  circulation(tup,ste(s,t)).. sum(g(ss,tt,ste),f(tup,g)) =e=
 197                              sum(g(ste,ss,tt),f(tup,g));
 198   
 199  demand(is,c)..     sum(z,timetable(z,is,c)) =l=
 200                     sum(tup, f(tup,is)*trainunitdata(tup,c));
 201   
 202  defmaxcars(is)..   maxcars =g=
 203                     sum(tup, f(tup,is)*trainunitdata(tup,'NumberCars'));
 204   
 205  defobj..           obj =e= sum((tup,on), f(tup,on)*trainunitdata(tup,'Cost'));
 206   
 207  Model nscirc /all/ ;
 208  option optcr=0;
 209   
 210  * If we do one type of trainunit at a time, we can tighten the demand equation
 211  f.lo(tu,is) = smax(c,ceil(sum(z,timetable(z,is,c)/trainunitdata(tu,c))));
 212   
 213  Parameter rep solution report;
 214   
 215  * Now trainunit tu1
 216  tup('tu1') = yes; tup('tu2') = no;
 217  solve nscirc using mip minimizing obj;
 218   
 219  rep('tu1 only',tup) = sum(on, f.l(tup,on));
 220  rep('tu1 only','Total cost') = obj.l;
