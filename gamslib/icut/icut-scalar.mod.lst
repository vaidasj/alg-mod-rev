C o m p i l a t i o n


  59  *  pick an objective function which will order the solutions
  60   
  61  obj..   z =e= sum(i, power(10,card(i)-ord(i))*x(i));
  62   
  63  cut(k)..      - sum(bu(k,i), x(i)) + sum(bl(k,i),x(i))
  64                + sum(bb(k,i), l(bb) + u(bb)) =g= cutrhs(k);
  65   
  66  cutu(bb(k,i))..  u(bb) =l= cutux(bb)*b(bb);
  67   
  68  cutl(bb(k,i))..  l(bb) =l= cutlx(bb)*(1-b(bb));
  69   
  70  cutul(bb(k,i)).. x(i)  =e= cuts(bb) + u(bb) - l(bb);
  71   
  72  model enum / all /;
  73   
  74  * get an initial solution and set bounds
  75   
  76  x.lo(i)   = 2;
  77  x.up(i)   = 4;
  78  x.fx('2') = 3;   ! fix one variable
  79  x.up('4') = 3;   ! only two values
  80   
  81  x.l(i)    = x.lo(i);
  82   
  83  k(kk) = no;                        ! make cut set empty
  84  ie(i) = yes$(x.lo(i)=x.up(i));     ! find fixed variables
  85  in(i) = yes - ie(i);               ! find free variables
  86   
  87  whatnext    = 1;  ! initial loop control
  88  enum.resusd = 0;  ! inital CPU used
  89   
  90  enum.reslim = 60; ! dont spend more than 60 seconds on on problem
  91   
  92  * We enumerate all solutions so we are happy with the first solution
  93  * the solver finds.
  94   
  95  enum.optcr  = 0; enum.optca = 1e06;
  96   
  97  loop(kk$whatnext,
  98     il(in) = yes$(x.l(in)=x.lo(in));         ! find variables at lower
  99     iu(in) = yes$(x.l(in)=x.up(in));         ! find variables at upper
 100     ib(in) = yes - ie(in) - iu(in) - il(in); ! find variables between
 101     k(kk)        = yes;   ! add
 102     bl(kk,il)    = yes;   !   cut
 103     bu(kk,iu)    = yes;   !      information
 104     bb(kk,ib)    = yes;   !         as needed
 105     cutux(kk,ib) = x.up(ib) - x.l(ib);
 106     cutlx(kk,ib) = x.l(ib) - x.lo(ib);
 107     cuts(kk,ib)  = x.l(ib);
 108     cutrhs(kk)   = 1 + sum(il, x.l(il)) - sum(iu, x.l(iu));
 109     report(kk,i) = x.l(i);                   ! save previous solution
 110     report(kk,'binaries') = card(bb);        ! remember binaries
 111     report(kk,'CPU time') = enum.resusd;     ! remember time
