 101   
 102  loop(c,
 103   
 104     q(p,s)     = dat('q',p,s) * multq(c);
 105     mu(s)      = mubase(s) * multmu(c);
 106     tau(p,s)   = q(p,s)*(dat('tau',p,s)-dat('kappa',p,s)*1e-5*mu(s));
 107     sigma(p,s) = delta(p)/q(p,s);
 108   
 109     pr.lo(p,s) = pi(p,s)/tau(p,s);
