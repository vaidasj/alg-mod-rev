  58  DefC..    C =e= coil/wire;
  59   
  60  DefK..    K =e= (4*C - 1) / (4*C - 4) + 0.615 / C;
  61   
  62  shear..   8*Pmax/PI*K*C/wire**2 =l= S;
  63   
  64  Defdel..  del =e= 8*N/G*power(C,3)/wire;
  65   
  66  freel..   Pmax*del + 1.05*(N+2)*wire =l= lmax;
  67   
  68  CoilWidth..  coil + wire =l= coilmax;
  69   
  70  DefWire..    wire =e= SUM(i, b(i)*y(i));
  71   
  72  SelectWire.. SUM(i, y(i)) =e= 1;
  73   
  74  model spring / all /;
  75   
  76  * set some bounds
  77   
  78  del.up  = delm/Pload;
  79  del.lo  = delw/(Pmax - Pload);
  80  wire.lo = smin(i, b(i));
  81  c.lo    = 1.1;
  82  coil.lo = 2*wire.lo;
  83  n.lo    = 1;
  84   
  85  * Starting point
  86  y.l(i) = 1/card(i);
  87  wire.l = SUM(i, b(i)*y.l(i));
  88  coil.l = coilmax - wire.l;
  89  c.l    = coil.l/wire.l;
  90   
  91  solve spring using minlp minimizing volume;


