 126   
 127  sprofitdef.. profit =e= -sum(crop, plantcost(crop)*x(crop))
 128                          +sum(s, p(s)*(
 129                                 -sum(cropr, purchprice(cropr)*ys(cropr,s))
 130                                 +sum(cropx, sellprice(cropx)*ws(cropx,s))
 131                               ));
 132   
 133  sreq(cropr,s).. syield(cropr,s)*x(cropr) + ys(cropr,s) - sum(sameas(cropx,cropr),ws(cropx,s)) =g= minreq(cropr);
 134   
 135  sbeets(s).. ws('beets1',s)+ws('beets2',s) =l= syield('sugarbeets',s)*x('sugarbeets');
 136  ws.up('beets1',s) = maxbeets1;
 137   
 138  model extform /sprofitdef,landuse,sreq,sbeets/;
 139  solve extform using lp maximizing profit;
