 100  *--------------------------------------------------------------------------
 101  * Extensive form stochastic model
 102  * This is a standard LP.
 103  *--------------------------------------------------------------------------
 104   
 105  set s scenarios / above, avg, below /;
 106   
 107  variables
 108    ws(cropx, s) crops sold under scenario s
 109    ys(cropr, s) crops purchased under scenario s;
 110  positive variable ws, ys;
 111   
 112  parameter p(s) probability;
 113  p(s) = 1/3;
 114  abort$(abs( sum(s,p(s)) - 1.0 ) > 0.001) "probabilities don't add up";
