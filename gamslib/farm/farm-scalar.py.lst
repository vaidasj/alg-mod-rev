 127   
 128  sprofitdef.. profit =e= -sum(crop, plantcost(crop)*x(crop))
 129                          +sum(s, p(s)*(
 130                                 -sum(cropr, purchprice(cropr)*ys(cropr,s))
 131                                 +sum(cropx, sellprice(cropx)*ws(cropx,s))
 132                               ));
 133   
 134  sreq(cropr,s).. syield(cropr,s)*x(cropr) + ys(cropr,s) - sum(sameas(cropx,cropr),ws(cropx,s)) =g= minreq(cropr);
 135   
 136  sbeets(s).. ws('beets1',s)+ws('beets2',s) =l= syield('sugarbeets',s)*x('sugarbeets');
