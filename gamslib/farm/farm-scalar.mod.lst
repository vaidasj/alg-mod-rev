 125   
 126  sprofitdef.. profit =e= -sum(crop, plantcost(crop)*x(crop))
 127                          +sum(s, p(s)*(
 128                                 -sum(cropr, purchprice(cropr)*ys(cropr,s))
 129                                 +sum(cropx, sellprice(cropx)*ws(cropx,s))
 130                               ));
 131   
 132  sreq(cropr,s).. syield(cropr,s)*x(cropr) + ys(cropr,s) - sum(sameas(cropx,cropr),ws(cropx,s)) =g= minreq(cropr);
 133   
 134  sbeets(s).. ws('beets1',s)+ws('beets2',s) =l= syield('sugarbeets',s)*x('sugarbeets');
