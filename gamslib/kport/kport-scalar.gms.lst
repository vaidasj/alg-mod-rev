 220  * SOS2 approximation of economies of scale function
 221  DEFSOSx(r)..  vR(r) =e= sum(j, vRj(r,j)*lambda(r,j));
 222   
 223  DEFSOSy(r)..  ESFvR(r) =e= sum(j, ESFvRj(r,j)*lambda(r,j));
 224   
 225  DEFSOSone(r).. sum(j, lambda(r,j)) =e= 1;
 226   
 227  DEFcIlp..  cInvest =e= sum (r, CSTI**ESF*ESFvR(r));
 228   
 229   
 230  rpi(r,p,i) = ord(i) <= ceil(log(max(1,nB.up(r,p)))/log(2)) + 1$nB.up(r,p);
 231   
 232  vRj(r,j) = (VMAX(r)-VMIN(r))*(ord(j)-1)/(card(j)-1) + VMIN(r);
 233  ESFvRj(r,j) = vRj(r,j)**ESF;
 234   
 235  Model PortfolioMIP  /TR, SPPx, RVLB, RVUB, DEFcF, DEFcIlp, DEFcT,
 236            CNPl0, CNPl1, CNPl2, CNPl3, CNPl4, DEFSOSx, DEFSOSy, DEFSOSone/;
