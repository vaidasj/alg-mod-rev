 114  * Determine scenario sets
 115  r(rR) = VMAX(rR)   > 0;
 116  p(pP) = DEMAND(pP) > 0;
 117   
 118  * definition of compact MINLP model
 119   
 120  Variables  cTotal       total  costs
 121             cInvest      invest cost
 122             cFixed       fix    costs
 123             f(rR,pP)     utilization rate
 124             vR(rR)       reactor volume in m^3
 125             pS(pP)       surplus production
 126             bvr(rR)      indicating whether reactor r is active
 127             nB(rR,pP)    number of batches of product p in reactor r
 128   
 129  Positive variables f,vR,pS; Integer variable nB; Binary Variables bvr;
 130   
 131   
 132  Equations DEFcT          total costs
 133            DEFcF          fix costs
 134            DEFcI          invest cost
 135            TR(rR)         production time of reactor r
 136            SPP(pP)        compute surplus production p
 137            RVUB(rR)       maximal volume of reactor r
 138            RVLB(rR)       minimal volume reactor r;
 139   
 140   
 141  * define the total cost
 142  DEFcT..  cTotal  =e= cFixed + cInvest;
 143  DEFcF..  cFixed  =e= sum (r, CSTF*bvr(r));
 144  DEFcI..  cInvest =e= sum (r, CSTI**ESF*vR(r)**ESF);
 145   
 146  * limit the total production time of reactor r
 147  TR(r)..    sum(p, PRODTIME(p)*nB(r,p) ) =l= WHRS*bvr(r);
 148   
 149  * compute the surplus production
 150  SPP(p)..   pS(p) =e= SUM(r, nB(r,p)*f(r,p)*vR(r))/DEMAND(p) - 1 ;
 151   
 152  * lower and upper bounds on reactor volume
 153  RVLB(r)..  vR(r) =g= VMIN(r)*bvr(r);
 154  RVUB(r)..  vR(r) =l= VMAX(r)*bvr(r);
 155   
 156  Model     portfolioMINLP / DEFcT, DEFcF, DEFcI, TR, SPP, RVLB, RVUB / ;
 157   
 158  f.lo(r,p) = 0.4; f.up(r,p)  = 1;  // bounds on the utilization rates
 159  pS.lo(p)  =   0; pS.up(p)   = 1;  // bounds on the surplus production
 160   
 161  * bounds on the number of batches
 162  nB.lo(r,p) = 0;
 163  nB.up(r,p) = min(WHRS/PRODTIME(p),floor(2*DEMAND(p)/(VMIN(r)*f.lo(r,p))));
 164  nB.up(r,p)$(2*DEMAND(p) < f.lo(r,p)*VMIN(r)) = 0;
 165   
 166  vR.l(rR) = 99;
