  30   
  31  set nh /i0 * i50/;
  32   
  33  alias(nh,i);
  34   
  35  scalars L  length of the suspended chain      / 4 /
  36          a  height of the chain at t=0 (left)  / 1 /
  37          b  height of the chain at t=1 (left)  / 3 /
  38          tf ODEs defined in [0 tf]             / 1 /
  39          h  uniform interval length
  40          n  number of subintervals
  41          tmin;
  42   
  43  if (b>a, tmin = 0.25 else tmin = 0.75);
  44  n = card(nh) - 1;
  45  h = tf/n;
