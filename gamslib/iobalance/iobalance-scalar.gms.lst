 116  *
 117  * --- 4. absolute deviation formulations result in LPs
 118  *        MAD Mean Absolute Deviations
 119  *        MAPE Mean absolute percentage error
 120  *        Linf Infinity norm
 121  positive variables
 122     ap(i,j)  positive deviation iation
 123     an(i,j)  negative deviation
 124     amax     maximum absilute dev;
 125  equations
 126     defabs(i,j)  absolute definition
 127     defmaxp(i,j) max positive
 128     defmaxn(i,j) max neagtive
 129     defmad       MAD definition
 130     defmade      Mean absolute percentage error
 131     deflinf      Infinity norm;
 132   
 133  defabs(i,j)..  a(i,j) - a0(i,j) =e= ap(i,j) - an(i,j);
 134  defmaxp(i,j).. a(i,j) - a0(i,j) =l=  amax;
 135  defmaxn(i,j).. a(i,j) - a0(i,j) =g= -amax;
 136   
 137  defmad..  obj =e=   1/sqr(card(i))*sum((i,j), ap(i,j) + an(i,j));
 138  defmade.. obj =e= 100/sqr(card(i))*sum((i,j),(ap(i,j) + an(i,j))/a0(i,j));
 139  defLinf.. obj =e=                  amax;
 140   
 141  models mMAD  / rowbal,colbal,defabs,defmad /
 142         mMADE / rowbal,colbal,defabs,defmade /
 143         mLinf / rowbal,colbal,defmaxp,defmaxn,deflinf /;
 144   
 145  solve mMAD  us lp min obj; report('MAD' ,i,j) = a.l(i,j);
 146  solve mMADe us lp min obj; report('MADE',i,j) = a.l(i,j);
 147  solve mLinf us lp min obj; report('Linf',i,j) = a.l(i,j);
 148  *
 149  * --- 5. Squared Deviations can be solved with powerful QP codes
 150  *        SD     squared deviations
 151  *        RSD    relative squared deviations
 152  equations defsd,defrsd;
 153   
 154  defsd..  obj =e= sum((i,j), sqr(a(i,j) + a0(i,j)));
 155  defrsd.. obj =e= sum((i,j), sqr(a(i,j) + a0(i,j))/a0(i,j));
 156   
 157  models mSD  / rowbal,colbal,defsd /
 158         mRSD / rowbal,colbal,defrsd /;
 159   
 160  solve mSD  us qcp min obj; report('SD'  ,i,j) = a.l(i,j);
 161  solve mRSD us qcp min obj; report('RSD' ,i,j) = a.l(i,j);
