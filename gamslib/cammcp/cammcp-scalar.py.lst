 219   delta(it)$m0(it) = pm0(it)/pd0(it)*(m0(it)/xxd0(it))**(1+rhoc(it)) ;
 220   delta(it) = delta(it)/(1+delta(it)) ;
 221   x0(i)     = pd0(i)*xxd0(i) + (pm0(i)*m0(i))$it(i) ;
 222   ac(it)    = x0(it)/(delta(it)*m0(it)**(-rhoc(it)) +
 223                  (1-delta(it))*xxd0(it)**(-rhoc(it)))**(-1/rhoc(it)) ;
 224   
 225  * get int0 from inteq, gamma from esupply, alphl from profitmax
 226   
 227   int0(i)   = sum(j, io(i,j)*xd0(j) );
 228   gamma(it) = 1/(1 + pd0(it)/pe0(it)*(e0(it)/xxd0(it))**(rhot(it) - 1) ) ;
 229   gamma(in) = 0;
 230   alphl(lc,i) = (wdist(i,lc) * wa0(lc) * xle(i,lc)) /(pva0(i)*xd0(i));
 231   
 232  * get ad from output, ld from  profitmax, at from cet
 233   
 234   qd(i)  = (xllb(i,"rural")**alphl("rural",i))*
 235                  (xllb(i,"urban-unsk")**alphl("urban-unsk",i))
 236             *(xllb(i,"urban-skil")**alphl("urban-skil",i))
 237             *(k0(i)**(1 - sum(lc, alphl(lc,i))) ) ;
 238   ad(i)  = xd0(i)/qd(i);
 239   ld(lc) = sum(i, (xd0(i)*pva0(i)*alphl(lc,i)/(wdist(i,lc)*wa0(lc)))$wdist(i,lc));
 240   at(it) = xd0(it)/( gamma(it)*e0(it)**rhot(it) + ( 1-gamma(it) )*xxd0(it)**rhot(it) )**(1/rhot(it)) ;
