 217   delta(it)$m0(it) = pm0(it)/pd0(it)*(m0(it)/xxd0(it))**(1+rhoc(it)) ;
 218   delta(it) = delta(it)/(1+delta(it)) ;
 219   x0(i)     = pd0(i)*xxd0(i) + (pm0(i)*m0(i))$it(i) ;
 220   ac(it)    = x0(it)/(delta(it)*m0(it)**(-rhoc(it)) +
 221                  (1-delta(it))*xxd0(it)**(-rhoc(it)))**(-1/rhoc(it)) ;
 222   
 223  * get int0 from inteq, gamma from esupply, alphl from profitmax
 224   
 225   int0(i)   = sum(j, io(i,j)*xd0(j) );
 226   gamma(it) = 1/(1 + pd0(it)/pe0(it)*(e0(it)/xxd0(it))**(rhot(it) - 1) ) ;
 227   gamma(in) = 0;
 228   alphl(lc,i) = (wdist(i,lc) * wa0(lc) * xle(i,lc)) /(pva0(i)*xd0(i));
 229   
 230  * get ad from output, ld from  profitmax, at from cet
 231   
 232   qd(i)  = (xllb(i,"rural")**alphl("rural",i))*
 233                  (xllb(i,"urban-unsk")**alphl("urban-unsk",i))
 234             *(xllb(i,"urban-skil")**alphl("urban-skil",i))
 235             *(k0(i)**(1 - sum(lc, alphl(lc,i))) ) ;
 236   ad(i)  = xd0(i)/qd(i);
 237   ld(lc) = sum(i, (xd0(i)*pva0(i)*alphl(lc,i)/(wdist(i,lc)*wa0(lc)))$wdist(i,lc));
 238   at(it) = xd0(it)/( gamma(it)*e0(it)**rhot(it) + ( 1-gamma(it) )*xxd0(it)**rhot(it) )**(1/rhot(it)) ;
