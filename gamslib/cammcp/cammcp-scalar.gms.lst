 205   pwm0(i) = pm0(i)/((1+tm0(i))*er) ;
 206   pwe0(i) = pe0(i)/((1+te(i))*er) ;
 207   pva0(i) = pd0(i) - sum(j, io(j,i)*pd0(j) ) - itax(i);
 208   xxd0(i) = xd0(i) - e0(i);
