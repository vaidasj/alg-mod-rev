 218   delta(it)$m0(it) = pm0(it)/pd0(it)*(m0(it)/xxd0(it))**(1+rhoc(it)) ;
 219   delta(it) = delta(it)/(1+delta(it)) ;
 220   x0(i)     = pd0(i)*xxd0(i) + (pm0(i)*m0(i))$it(i) ;
 221   ac(it)    = x0(it)/(delta(it)*m0(it)**(-rhoc(it)) +
 222                  (1-delta(it))*xxd0(it)**(-rhoc(it)))**(-1/rhoc(it)) ;
 223   
 224  * get int0 from inteq, gamma from esupply, alphl from profitmax
 225   
 226   int0(i)   = sum(j, io(i,j)*xd0(j) );
 227   gamma(it) = 1/(1 + pd0(it)/pe0(it)*(e0(it)/xxd0(it))**(rhot(it) - 1) ) ;
 228   gamma(in) = 0;
 229   alphl(lc,i) = (wdist(i,lc) * wa0(lc) * xle(i,lc)) /(pva0(i)*xd0(i));
 230   
 231  * get ad from output, ld from  profitmax, at from cet
 232   
 233   qd(i)  = (xllb(i,"rural")**alphl("rural",i))*
 234                  (xllb(i,"urban-unsk")**alphl("urban-unsk",i))
 235             *(xllb(i,"urban-skil")**alphl("urban-skil",i))
 236             *(k0(i)**(1 - sum(lc, alphl(lc,i))) ) ;
 237   ad(i)  = xd0(i)/qd(i);
 238   ld(lc) = sum(i, (xd0(i)*pva0(i)*alphl(lc,i)/(wdist(i,lc)*wa0(lc)))$wdist(i,lc));
 239   at(it) = xd0(it)/( gamma(it)*e0(it)**rhot(it) + ( 1-gamma(it) )*xxd0(it)**rhot(it) )**(1/rhot(it)) ;
