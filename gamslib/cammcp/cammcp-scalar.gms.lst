 193   dstr(i)    = zz("dstr",i);
 194   xllb(i,lc) = xle(i,lc) + (1 - sign(xle(i,lc)));
 195   
 196   m0(i)   = zz("m0",i);
 197   it(i)   = yes$m0(i);
 198   in(i)   = not it(i);
 199   e0(i)   = zz("e0",i);
 200   xd0(i)  = zz("xd0",i);
 201   k0(i)   = zz("k",i);
 202   pd0(i)  = zz("pd0",i);
 203   pm0(i)  = pd0(i) ;
