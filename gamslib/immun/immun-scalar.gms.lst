  66    kl        dollar duration of liability
  67    ql        dollar convexity of liability;
  68   
  69  rl(tl) =  liab(tl,"liabrate");
  70  pl     =  sum( tl, liab(tl,'liability')
  71                   * exp(-rl(tl)*liab(tl,'term')) );
  72  kl     = -sum( tl, liab(tl,'liability')
  73                   * exp(-rl(tl)*liab(tl,'term'))
  74                   * liab(tl,'term')  );
  75  ql     =  sum( tl, liab(tl,'liability')
  76                   * exp(-rl(tl)*liab(tl,'term'))
  77                   * sqr(liab(tl,'term')) );
