 153   
 154  cf(ts,i) =  sdata(ts,i) * 10;
 155  r(i)     =  sdata('yield',i) / 100;
 156  p(i)     =  (sdata('price',i) + sdata('accr',i)) * 10;
 157  pv(i)    =  sum( ts, cf(ts,i) * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 158  k(i)     = -sum( ts, cf(ts,i) *
 159               stime(ts,'term') * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 160  q(i)     =  sum( ts, cf(ts,i) * (stime(ts,'term')+1) *
 161               stime(ts,'term') * (1+r(i)/2)**(-2*stime(ts,'term')-2) );
