 238   
 239  Parameters
 240    sfac(ts,j)    interpolated factor loadings on term of securities
 241    lfac(tl,j)    interpolated factor loadings on term of liabilities
 242    f(i,j)        factor sensitivities of securities
 243    fl(j)         factor sensitivities of liabilities ;
 244   
 245   
 246  Loop( (ts,tf)$(a(tf,"term") eq trunc(stime(ts,"term"))),
 247    sfac(ts,j) = ( a(tf,j) +
 248                   (a(tf+1,j)-a(tf,j)) * (stime(ts,"term")-a(tf,"term")) )
 249                     / 100 );
 250   
 251  Loop( (tl,tf)$(a(tf,"term") eq trunc(liab(tl,"term"))),
 252    lfac(tl,j) = ( a(tf,j) +
 253                   (a(tf+1,j)-a(tf,j)) * (liab(tl,"term")-a(tf,"term")) )
 254                     / 100 );
 255   
 256   
 257  f(i,j) = - sum( ts, sfac(ts,j) * cf(ts,i) *
 258               stime(ts,"term") * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 259   
 260  fl(j)  = - sum( tl, lfac(tl,j) * liab(tl,"liability") *
 261               liab(tl,"term")  * exp(-rl(tl)*liab(tl,"term")) );
