 237   
 238  Parameters
 239    sfac(ts,j)    interpolated factor loadings on term of securities
 240    lfac(tl,j)    interpolated factor loadings on term of liabilities
 241    f(i,j)        factor sensitivities of securities
 242    fl(j)         factor sensitivities of liabilities ;
 243   
 244   
 245  Loop( (ts,tf)$(a(tf,"term") eq trunc(stime(ts,"term"))),
 246    sfac(ts,j) = ( a(tf,j) +
 247                   (a(tf+1,j)-a(tf,j)) * (stime(ts,"term")-a(tf,"term")) )
 248                     / 100 );
 249   
 250  Loop( (tl,tf)$(a(tf,"term") eq trunc(liab(tl,"term"))),
 251    lfac(tl,j) = ( a(tf,j) +
 252                   (a(tf+1,j)-a(tf,j)) * (liab(tl,"term")-a(tf,"term")) )
 253                     / 100 );
 254   
 255   
 256  f(i,j) = - sum( ts, sfac(ts,j) * cf(ts,i) *
 257               stime(ts,"term") * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 258   
 259  fl(j)  = - sum( tl, lfac(tl,j) * liab(tl,"liability") *
 260               liab(tl,"term")  * exp(-rl(tl)*liab(tl,"term")) );
