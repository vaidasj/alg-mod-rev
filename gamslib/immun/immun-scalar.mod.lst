 153  cf(ts,i) =  sdata(ts,i) * 10;
 154  r(i)     =  sdata('yield',i) / 100;
 155  p(i)     =  (sdata('price',i) + sdata('accr',i)) * 10;
 156  pv(i)    =  sum( ts, cf(ts,i) * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
