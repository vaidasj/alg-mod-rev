 154   
 155  cf(ts,i) =  sdata(ts,i) * 10;
 156  r(i)     =  sdata('yield',i) / 100;
 157  p(i)     =  (sdata('price',i) + sdata('accr',i)) * 10;
 158  pv(i)    =  sum( ts, cf(ts,i) * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 159  k(i)     = -sum( ts, cf(ts,i) *
 160               stime(ts,'term') * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 161  q(i)     =  sum( ts, cf(ts,i) * (stime(ts,'term')+1) *
 162               stime(ts,'term') * (1+r(i)/2)**(-2*stime(ts,'term')-2) );
