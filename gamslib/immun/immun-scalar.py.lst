  70  rl(tl) =  liab(tl,"liabrate");
  71  pl     =  sum( tl, liab(tl,'liability')
  72                   * exp(-rl(tl)*liab(tl,'term')) );
  73  kl     = -sum( tl, liab(tl,'liability')
  74                   * exp(-rl(tl)*liab(tl,'term'))
  75                   * liab(tl,'term')  );
