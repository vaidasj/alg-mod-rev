 239   
 240  Parameters
 241    sfac(ts,j)    interpolated factor loadings on term of securities
 242    lfac(tl,j)    interpolated factor loadings on term of liabilities
 243    f(i,j)        factor sensitivities of securities
 244    fl(j)         factor sensitivities of liabilities ;
 245   
 246   
 247  Loop( (ts,tf)$(a(tf,"term") eq trunc(stime(ts,"term"))),
 248    sfac(ts,j) = ( a(tf,j) +
 249                   (a(tf+1,j)-a(tf,j)) * (stime(ts,"term")-a(tf,"term")) )
 250                     / 100 );
 251   
 252  Loop( (tl,tf)$(a(tf,"term") eq trunc(liab(tl,"term"))),
 253    lfac(tl,j) = ( a(tf,j) +
 254                   (a(tf+1,j)-a(tf,j)) * (liab(tl,"term")-a(tf,"term")) )
 255                     / 100 );
 256   
 257   
 258  f(i,j) = - sum( ts, sfac(ts,j) * cf(ts,i) *
 259               stime(ts,"term") * (1+r(i)/2)**(-2*stime(ts,'term')-1) );
 260   
 261  fl(j)  = - sum( tl, lfac(tl,j) * liab(tl,"liability") *
 262               liab(tl,"term")  * exp(-rl(tl)*liab(tl,"term")) );
