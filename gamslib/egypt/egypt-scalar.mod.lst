 711    demdat(cn,"dem-a") =alpha(cn);  demdat(cn,"dem-b") = beta(cn);
 712    pmax(cn) =  price(cn)*high;    pmin(cn) = price(cn)*low ;
 713    qmin(cn) = (pmax(cn)-alpha(cn))/beta(cn);
 714    qmax(cn) = (pmin(cn)-alpha(cn))/beta(cn);
 715    incr(cn) = (qmax(cn)-qmin(cn) )/(card(g)-1);
 716   
 717    qs(cn,g)=qmin(cn) + incr(cn)*(ord(g)-1);
 718    ws(cn,g)=alpha(cn)*qs(cn,g)+ .5*beta(cn)*sqr(qs(cn,g));
 719    rs(cn,g)=alpha(cn)*qs(cn,g)+    beta(cn)*sqr(qs(cn,g));
