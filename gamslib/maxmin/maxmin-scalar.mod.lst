C o m p i l a t i o n


  58   
  59   
  60  scalar p; p = 0;              // Pinter's
  61  loop((n,d),                   // original
  62     p = round(mod(p,10)) + 1;  // nominal
  63     point.l(n,d) = p/10 );     // point  0.1,.2, ... 1.0, 0.1, ...
  64   
  65  point.lo(n,d)     = 0;
  66  point.up(n,d)     = 1;
  67  point.l (n,d)     = uniform(0,1);
  68  dist.l(low(n,nn)) = sqrt(sqr(point.l(n,'x')-point.l(nn,'x')) + sqr(point.l(n,'y')-point.l(nn,'y')));
  69   
  70  point.fx('p1',d) = 0;           // fix one point
  71   
  72  parameter bnd lower bound on objective;
  73   
  74  bnd = 1/max(ceil(sqrt(card(n)))-1,1); display bnd;
  75   
  76  option limcol=0,limrow=0; if(card(n) > 9, option solprint=off;);
  77   
  78  * for experimentation we will combine different model version
  79  * with different bounds and starting points
  80  *
  81  * dist.lo(low) =  -inf;
  82  * dist.lo(low) =  0;
  83  * dist.lo(low) =  0.01;
  84  * dist.lo(low) = bnd/2;
  85  * dist.lo(low) = bnd;
  86  *
  87  * solve maxmin1  max mindist us nlp;
  88  * solve maxmin1a max mindist us nlp;
  89  * solve maxmin2  max mindist us dnlp;
  90  * solve maxmin2a max mindist us dnlp;
  91   
  92  * maxmin2 and maxmin2a without bounds are well suited for LGO
  93  * maxmin1a with bounds is well suited for conopt3 (bounds 200 point is ok)
  94   
  95  solve maxmin1a max mindist us dnlp;
  96   
  97  display bnd,mindist.l, point.l;


