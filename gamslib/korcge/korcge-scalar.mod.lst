 372   
 373   pmdef(it)..       pm(it) =e= pwm(it)*er*(1 + tm(it) + pr) ;
 374   
 375   pedef(it)..       pe(it) =e= pwe(it)*(1 + te(it))*er ;
 376   
 377   absorption(i)..   p(i)*x(i) =e= pd(i)*xxd(i) + (pm(i)*m(i))$it(i) ;
 378   
 379   sales(i)..        px(i)*xd(i) =e= pd(i)*xxd(i) + (pe(i)*e(i))$it(i) ;
 380   
 381   actp(i)..         px(i)*(1-itax(i)) =e= pva(i) + sum(j, io(j,i)*p(j) ) ;
 382   
 383   pkdef(i)..        pk(i) =e= sum(j, p(j)*imat(j,i) );
 384   
 385   pindexdef..       pindex =e= sum(i, pwts(i)*p(i) ) ;
 386   
 387   
 388  *output and factors of production block
 389   
 390   activity(i)..     xd(i) =e= ad(i) * prod(lc$wdist(i,lc), l(i,lc)**alphl(i,lc) )
 391                               *k(i)**(1 - sum(lc, alphl(i,lc)) ) ;
 392   
 393   profitmax(i,lc)$wdist(i,lc).. wa(lc)*wdist(i,lc)*l(i,lc) =e=
 394                                    xd(i)*pva(i)*alphl(i,lc) ;
 395   
 396   lmequil(lc)..     sum(i, l(i,lc)) =e= ls(lc) ;
 397   
 398   cet(it)..         xd(it) =e= at(it)*( gamma(it)*e(it)**rhot(it) +
 399                     ( 1-gamma(it) )*xxd(it)**rhot(it) )**(1/rhot(it)) ;
 400   
 401   esupply(it)..     e(it)/xxd(it) =e= ( pe(it)/pd(it)*(1 - gamma(it))/gamma(it) )
 402                                       **(1/(rhot(it)-1) ) ;
 403   
 404   armington(it)..   x(it) =e= ac(it)*(delta(it)*m(it)**(-rhoc(it)) +
 405                     (1-delta(it))*xxd(it)**(-rhoc(it)))**(-1/rhoc(it)) ;
 406   
 407   costmin(it)..     m(it)/xxd(it) =e= ( pd(it)/pm(it)*delta(it)/(1-delta(it)) )**
 408                                       (1/(1 + rhoc(it))) ;
