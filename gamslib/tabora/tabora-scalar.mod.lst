 131   
 132   wb(t)..   yw*sum(i, w(t,i)) + sum(a, yv(a)*v(t-ord(a))) =g= wrc*x(t,"tobacco") + dwr*nfam;
 133   
 134   wa(i)..   sum(t, w(t,i)) =l= fa(i) ;
 135   
 136   lb(t,m).. ld("timber-1",m)*v(t) + ld("timber-2",m)*v(t-1) + sum(c, ld(c,m)*x(t,c)) + lc(t,m)$mc(m) =l= labor(m);
 137   
 138   lw(t)..   sum(mc, lc(t,mc)) =e= sum(i, labw(i)*w(t,i)) + sum(a, labvc*yv(a)*v(t-ord(a)));
 139   
 140   mm(t)..   yc("maize")*(x(t,"maize") + matr*mat(t)) =g= dmaize;
 141   
 142   ttb(t)..  sum(a , yv(a)*v(t+(card(t)-ord(a))) ) =l= wr;
 143   
 144   matd1(t-1).. mat(t) =l= x(t-1,"tobacco");
 145   
 146   matd2(t)..   mat(t) =l= x(t,"maize");
 147   
 148   rd(t).. rev(t)  =e= (sum(c, cr(c)*x(t,c)) + matr*cr("maize")*mat(t))/1000;
 149   
 150   cd(t).. cost(t) =e= (sum(c, cc(c)*x(t,c)) + tc*v(t) + resw*whd*sum(mc, lc(t,mc)) + sum(i, tr*yw*dist(i)*w(t,i)))/1000;
 151   
 152   od..    income =e= sum(t, delt(t)*(rev(t) - cost(t) + vr(t)*v(t)/1000) );
 153   
 154   mat.up("y01") = tob;
