  69  *
  70  * Cuts to cut off binary solution
  71  *
  72  set cuts / c1*c5 /, cc(cuts) dynamic cuts;
  73  parameter sol solutions to cut off;
  74  equation cut(cuts);
  75   
  76  cut(cc)..     sum((x,y,z)$sol(cc,x,y,z), core(x,y,z))
  77            +   sum((x,y,z)$(sol(cc,x,y,z)=0), 1-core(x,y,z))
  78            =l= card(x)*card(y)*card(z)-1;
