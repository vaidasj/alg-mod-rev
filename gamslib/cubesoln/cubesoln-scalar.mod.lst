  68  *
  69  * Cuts to cut off binary solution
  70  *
  71  set cuts / c1*c5 /, cc(cuts) dynamic cuts;
  72  parameter sol solutions to cut off;
  73  equation cut(cuts);
  74   
  75  cut(cc)..     sum((x,y,z)$sol(cc,x,y,z), core(x,y,z))
  76            +   sum((x,y,z)$(sol(cc,x,y,z)=0), 1-core(x,y,z))
  77            =l= card(x)*card(y)*card(z)-1;
