model definition


 351   
 352   Variables  xcrop(s,f)     paddy land cropping activities     (mu)
 353              xupland(ca)    upland cropping activities         (mu)
 354              xpig(p)        pig raising activities            (ton)
 355              xfeed(g)       grain feed mixing                 (ton)
 356              xfert(ca)      fertilization activities           (kg)
 357              purchase(ca)   purchasing activities             (ton)
 358              sales(ca)      quota sales                       (ton)
 359              aqsales(ca)    above quota sales                 (ton)
 360              ccost          cash cost                        (yuan)
 361              income         brigade income                   (yuan)
 362   
 363   Positive variables xcrop, xupland, xpig, xfeed, xfert, purchase, sales, aqsales;
 364   
 365   Equations  labor(t)     labor balance               (gong per day)
 366              mb(ca)       material balance                     (ton)
 367              fert(en)     nutrient and humus balance           (ton)
 368              chemn        chemical nitrogen allocation         (ton)
 369              landp        paddy land constraint                 (mu)
 370              landu        upland land constraint                (mu)
 371              gmseed       green manure seed requirements        (mu)
 372              grainq       grain quota sales                    (ton)
 373              cdef         cash cost definition                (yuan)
 374              incdef       income definition                   (yuan);
 375   
 376   mb(ca)..  sum((s,f)$ss(s,f), syield(ca,s,f)*xcrop(s,f)) + sum(p, pigio(ca,p)*xpig(p)) + purchase(ca)$cp(ca)
 377           + sum(g, gio(ca,g)*xfeed(g)) + yieldu(ca)*xupland(ca)$cu(ca)
 378        =g=  (sales(ca) + aqsales(ca))$cs(ca) + sum(cf, crec(ca,cf)*xfert(cf)) ;
 379   
 380   labor(t)..  sum((s,f)$ss(s,f), lab(t,s)*xcrop(s,f)) =l= (1+labj(t))*lsup;
 381   
 382   fert(en)..  sum((s,f)$ss(s,f), sreq(s,en,f)*xcrop(s,f)) + .001*sum(cu, nup(cu,en)*xupland(cu))
 383           =l= .01*sum(cf, enc(cf,en)*xfert(cf));
 384   
 385   chemn..     sum((s,f)$ss(s,f), schem(s)*xcrop(s,f)) + sum(cs, chemnall(cs,"qsa")*sales(cs)
 386             + chemnall(cs,"aqsa")*aqsales(cs)) =g= purchase("amm-bi");
 387   
 388   landp..   sum((s,f)$ss(s,f), xcrop(s,f)) =l= paddy;
 389   
 390   landu..   sum(cu, xupland(cu)) =l= upland;
 391   
 392   gmseed..  sum((s,f)$ss(s,f), ((1-.16)*mcp(s,"gm-seeds") - .16*mcp(s,"g-manure"))*xcrop(s,f)) =g= 0 ;
 393   
 394   grainq..  sum(g, sales(g)) =g= grainquota;
 395   
 396   cdef..    ccost =e= sum((s,f)$ss(s,f), cxcrop(s)*xcrop(s,f)) + sum(p, cxpig(p)*xpig(p))
 397                     + sum(cf, cxfert(cf)*xfert(cf)) + sum(cp, purdata(cp,"price")*purchase(cp));
 398   
 399   incdef.. income =e= sum(cs, cdata(cs,"proc-price")*sales(cs) + aqsprice(cs)*aqsales(cs)) - ccost ;
 400   
 401   Model china brigade model  / all / ;
 402   
 403   sales.lo(cs) = cdata(cs,"quota-sale");
