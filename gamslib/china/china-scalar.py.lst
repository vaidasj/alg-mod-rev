model definition


 352   
 353   Variables  xcrop(s,f)     paddy land cropping activities     (mu)
 354              xupland(ca)    upland cropping activities         (mu)
 355              xpig(p)        pig raising activities            (ton)
 356              xfeed(g)       grain feed mixing                 (ton)
 357              xfert(ca)      fertilization activities           (kg)
 358              purchase(ca)   purchasing activities             (ton)
 359              sales(ca)      quota sales                       (ton)
 360              aqsales(ca)    above quota sales                 (ton)
 361              ccost          cash cost                        (yuan)
 362              income         brigade income                   (yuan)
 363   
 364   Positive variables xcrop, xupland, xpig, xfeed, xfert, purchase, sales, aqsales;
 365   
 366   Equations  labor(t)     labor balance               (gong per day)
 367              mb(ca)       material balance                     (ton)
 368              fert(en)     nutrient and humus balance           (ton)
 369              chemn        chemical nitrogen allocation         (ton)
 370              landp        paddy land constraint                 (mu)
 371              landu        upland land constraint                (mu)
 372              gmseed       green manure seed requirements        (mu)
 373              grainq       grain quota sales                    (ton)
 374              cdef         cash cost definition                (yuan)
 375              incdef       income definition                   (yuan);
 376   
 377   mb(ca)..  sum((s,f)$ss(s,f), syield(ca,s,f)*xcrop(s,f)) + sum(p, pigio(ca,p)*xpig(p)) + purchase(ca)$cp(ca)
 378           + sum(g, gio(ca,g)*xfeed(g)) + yieldu(ca)*xupland(ca)$cu(ca)
 379        =g=  (sales(ca) + aqsales(ca))$cs(ca) + sum(cf, crec(ca,cf)*xfert(cf)) ;
 380   
 381   labor(t)..  sum((s,f)$ss(s,f), lab(t,s)*xcrop(s,f)) =l= (1+labj(t))*lsup;
 382   
 383   fert(en)..  sum((s,f)$ss(s,f), sreq(s,en,f)*xcrop(s,f)) + .001*sum(cu, nup(cu,en)*xupland(cu))
 384           =l= .01*sum(cf, enc(cf,en)*xfert(cf));
 385   
 386   chemn..     sum((s,f)$ss(s,f), schem(s)*xcrop(s,f)) + sum(cs, chemnall(cs,"qsa")*sales(cs)
 387             + chemnall(cs,"aqsa")*aqsales(cs)) =g= purchase("amm-bi");
 388   
 389   landp..   sum((s,f)$ss(s,f), xcrop(s,f)) =l= paddy;
 390   
 391   landu..   sum(cu, xupland(cu)) =l= upland;
 392   
 393   gmseed..  sum((s,f)$ss(s,f), ((1-.16)*mcp(s,"gm-seeds") - .16*mcp(s,"g-manure"))*xcrop(s,f)) =g= 0 ;
 394   
 395   grainq..  sum(g, sales(g)) =g= grainquota;
 396   
 397   cdef..    ccost =e= sum((s,f)$ss(s,f), cxcrop(s)*xcrop(s,f)) + sum(p, cxpig(p)*xpig(p))
 398                     + sum(cf, cxfert(cf)*xfert(cf)) + sum(cp, purdata(cp,"price")*purchase(cp));
 399   
 400   incdef.. income =e= sum(cs, cdata(cs,"proc-price")*sales(cs) + aqsprice(cs)*aqsales(cs)) - ccost ;
 401   
 402   Model china brigade model  / all / ;
 403   
 404   sales.lo(cs) = cdata(cs,"quota-sale");
