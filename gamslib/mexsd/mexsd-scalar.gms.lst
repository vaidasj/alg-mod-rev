 322   

 323   mb(c,i,t)..    sum(p, a(c,p)*z(p,i,t)) + u(c,i,t)$cr(c) + sum(im, xm(c,im,i,t))$cm(c) + vr(c,i,t)$cv(c)

 324                + sum(ip, xn(c,ip,i,t))$ci(c) =g= sum(ip, xn(c,i,ip,t))$ci(c) + sum(j, x(c,i,j,t))$cf(c) + e(c,i,t)$ce(c);

