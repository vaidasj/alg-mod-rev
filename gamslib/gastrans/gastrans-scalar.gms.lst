 113  option arep:6:3:1; display arep,as,ap;
 114   
 115  Variables f(a,i,j)   Arc flow (1e6 SCM)
 116            s(i)       supply - demand (1e6 SCM)
 117            pi(i)      squared pressure
 118            sc         supply cost
 119  ;
 120   
 121  Equations flowbalance(i)    flow conservation
 122            weymouthp(a,i,j)  flow pressure relationship - passive
 123            weymoutha(a,i,j)  flow pressure relationship - active
 124            defsc             definition of supply cost
 125  ;
 126   
 127  flowbalance(i).. sum(aij(a,i,j), f(aij)) =e= sum(aij(a,j,i), f(aij)) + s(i);
 128   
 129  weymouthp(aij(ap,i,j)).. signpower(f(aij),2) =e= c2(aij)*(pi(i)-pi(j));
 130   
 131  weymoutha(aij(as,i,j))..       - sqr(f(aij)) =g= c2(aij)*(pi(i)-pi(j));
 132   
 133  defsc.. sc =e= sum(i, ndata(i,'c')*s(i));
 134   
 135  * supply, demand, pressure, and flow bounds
 136  s.lo(i)  = ndata(i,'slo');
 137  s.up(i)  = ndata(i,'sup');
 138  pi.lo(i) = sqr(ndata(i,'plo'));
 139  pi.up(i) = sqr(ndata(i,'pup'));
 140  f.lo(aij(as,i,j)) = 0;
 141   
 142  * initialize flow to avoid getting trapped at signpower(0,2)
 143  f.l(aij) = uniform(-1,1);
 144   
 145  model gastrans / flowbalance, weymouthp, weymoutha, defsc /;
 146   
 147  solve gastrans using nlp min sc;
 148   
 149  display f.l;
