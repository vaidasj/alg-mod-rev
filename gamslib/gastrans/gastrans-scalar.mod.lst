 112  option arep:6:3:1; display arep,as,ap;
 113   
 114  Variables f(a,i,j)   Arc flow (1e6 SCM)
 115            s(i)       supply - demand (1e6 SCM)
 116            pi(i)      squared pressure
 117            sc         supply cost
 118  ;
 119   
 120  Equations flowbalance(i)    flow conservation
 121            weymouthp(a,i,j)  flow pressure relationship - passive
 122            weymoutha(a,i,j)  flow pressure relationship - active
 123            defsc             definition of supply cost
 124  ;
 125   
 126  flowbalance(i).. sum(aij(a,i,j), f(aij)) =e= sum(aij(a,j,i), f(aij)) + s(i);
 127   
 128  weymouthp(aij(ap,i,j)).. signpower(f(aij),2) =e= c2(aij)*(pi(i)-pi(j));
 129   
 130  weymoutha(aij(as,i,j))..       - sqr(f(aij)) =g= c2(aij)*(pi(i)-pi(j));
 131   
 132  defsc.. sc =e= sum(i, ndata(i,'c')*s(i));
 133   
 134  * supply, demand, pressure, and flow bounds
 135  s.lo(i)  = ndata(i,'slo');
 136  s.up(i)  = ndata(i,'sup');
 137  pi.lo(i) = sqr(ndata(i,'plo'));
 138  pi.up(i) = sqr(ndata(i,'pup'));
 139  f.lo(aij(as,i,j)) = 0;
 140   
 141  * initialize flow to avoid getting trapped at signpower(0,2)
 142  f.l(aij) = uniform(-1,1);
 143   
 144  model gastrans / flowbalance, weymouthp, weymoutha, defsc /;
 145   
 146  solve gastrans using nlp min sc;
 147   
 148  display f.l;
