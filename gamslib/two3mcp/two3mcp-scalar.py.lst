  60   
  61  equations
  62          fmkt(f)         factor market,
  63          cmkt(s)         commodity market,
  64          profit(s)       zero profit,
  65          income(h)       income equation;
  66   
  67  fmkt(f)..       sum(h, e(f,h)) =g=
  68                  sum(s, Y(s) * phi(s)**(sigma(s)-1) *
  69                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  70                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  71                  **(1/(1-sigma(s)))/phi(s))
  72                  / (W(f) * (1 + t(f,s))))**sigma(s));
  73   
  74  cmkt(s)..       Y(s) =g= sum(h,
  75                  (i(h)/sum(ss, alpha(ss,h) * P(ss)**(1-sigmac(h)))) *
  76                  alpha(s,h) * (1 /P(s))**sigmac(h));
  77   
  78  profit(s)..     sum(f, delta(f,s)**sigma(s) *
  79                  (W(f)*(1 + t(f,s)))**(1 - sigma(s)))**(1/(1-sigma(s)))/phi(s)
  80                  =g= P(s);
  81   
  82  income(h)..     I(h) =g= sum(f, E(f,h) * W(f)) + tshr(h) *
  83                  sum((s,f), t(f,s) * W(f) * Y(s) * phi(s)**(sigma(s)-1) *
  84                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  85                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  86                  **(1/(1-sigma(s)))/phi(s))/(W(f) * (1 + t(f,s))))**sigma(s));
  87   
  88  model jel / fmkt.W, cmkt.P, profit.Y, income.I/;
