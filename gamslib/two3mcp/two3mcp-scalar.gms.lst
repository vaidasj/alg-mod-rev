  59   
  60  equations
  61          fmkt(f)         factor market,
  62          cmkt(s)         commodity market,
  63          profit(s)       zero profit,
  64          income(h)       income equation;
  65   
  66  fmkt(f)..       sum(h, e(f,h)) =g=
  67                  sum(s, Y(s) * phi(s)**(sigma(s)-1) *
  68                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  69                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  70                  **(1/(1-sigma(s)))/phi(s))
  71                  / (W(f) * (1 + t(f,s))))**sigma(s));
  72   
  73  cmkt(s)..       Y(s) =g= sum(h,
  74                  (i(h)/sum(ss, alpha(ss,h) * P(ss)**(1-sigmac(h)))) *
  75                  alpha(s,h) * (1 /P(s))**sigmac(h));
  76   
  77  profit(s)..     sum(f, delta(f,s)**sigma(s) *
  78                  (W(f)*(1 + t(f,s)))**(1 - sigma(s)))**(1/(1-sigma(s)))/phi(s)
  79                  =g= P(s);
  80   
  81  income(h)..     I(h) =g= sum(f, E(f,h) * W(f)) + tshr(h) *
  82                  sum((s,f), t(f,s) * W(f) * Y(s) * phi(s)**(sigma(s)-1) *
  83                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  84                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  85                  **(1/(1-sigma(s)))/phi(s))/(W(f) * (1 + t(f,s))))**sigma(s));
  86   
  87  model jel / fmkt.W, cmkt.P, profit.Y, income.I/;
