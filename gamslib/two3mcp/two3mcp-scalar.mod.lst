  58   
  59  equations
  60          fmkt(f)         factor market,
  61          cmkt(s)         commodity market,
  62          profit(s)       zero profit,
  63          income(h)       income equation;
  64   
  65  fmkt(f)..       sum(h, e(f,h)) =g=
  66                  sum(s, Y(s) * phi(s)**(sigma(s)-1) *
  67                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  68                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  69                  **(1/(1-sigma(s)))/phi(s))
  70                  / (W(f) * (1 + t(f,s))))**sigma(s));
  71   
  72  cmkt(s)..       Y(s) =g= sum(h,
  73                  (i(h)/sum(ss, alpha(ss,h) * P(ss)**(1-sigmac(h)))) *
  74                  alpha(s,h) * (1 /P(s))**sigmac(h));
  75   
  76  profit(s)..     sum(f, delta(f,s)**sigma(s) *
  77                  (W(f)*(1 + t(f,s)))**(1 - sigma(s)))**(1/(1-sigma(s)))/phi(s)
  78                  =g= P(s);
  79   
  80  income(h)..     I(h) =g= sum(f, E(f,h) * W(f)) + tshr(h) *
  81                  sum((s,f), t(f,s) * W(f) * Y(s) * phi(s)**(sigma(s)-1) *
  82                  (delta(f,s) * (sum(ff, delta(ff,s)**sigma(s) *
  83                  (W(ff)*(1 + t(ff,s)))**(1 - sigma(s)))
  84                  **(1/(1-sigma(s)))/phi(s))/(W(f) * (1 + t(f,s))))**sigma(s));
  85   
  86  model jel / fmkt.W, cmkt.P, profit.Y, income.I/;
  87   
  88  *       compute solution for this dimension problem:
  89   
  90  W.lo(f) = 0.0001;
  91  P.lo(s) = 0.0001;
  92   
  93  W.l(f) = 1;
  94  P.l(s) = 1;
  95  Y.l(s) = 10;
  96  I.l(h) = sum(f, W.l(f) * E(f,h));
  97   
  98  *       solve the reference case:
  99   
 100  W.fx("labor") = 1;
 101  solve jel using mcp;
 102   
 103  *       apply tax in test problem:
 104  tshr("rich") = 0.4;
 105  tshr("poor") = 1 - tshr("rich");
 106  t("capital","mfrs") = 0.5;
 107  solve jel using mcp;


