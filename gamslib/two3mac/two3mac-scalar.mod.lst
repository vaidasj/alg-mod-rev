  58   
  59  positive
  60  variables
  61          W(f)    factor price,
  62          P(s)    commodity price,
  63          Y(s)    production level;
  64   
  65   
  66   
  67  equations
  68          fmkt(f)         factor market,
  69          cmkt(s)         commodity market,
  70          profit(s)       zero profit;
  71   
  72  *       Factor supply (endowments) equals factor demand:
  73   
  74  fmkt(f)..       sum(h, e(f,h)) =g= sum(s, (delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]*Y(s));
  75   
  76  *       Commodity output equals commodity demand:
  77   
  78  cmkt(s)..       Y(s) =g= sum(h, (sum{f, e[f,h]*W[f]} + tshr[h]*sum{s.local, sum{f.local, t[f,s]*W[f]*Y[s]*(delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]}})*alpha[s
      ,h]*sum{s.local, alpha[s,h]*P[s]**(sigmac[h]-1)}*P[s]**(-sigmac[h]));
  79   
  80  *       Unit cost equals market price:
  81   
  82  profit(s)..  sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s] =g= P(s);
  83   
  84  parameter DL(s,h);
  85   
  86   
  87  model jel / fmkt.W, cmkt.P, profit.Y/;
  88   
  89  *       compute solution for this dimension problem:
  90   
  91  W.lo(f) = 0.0001;
  92  P.lo(s) = 0.0001;
  93   
  94  W.l(f) = 1;
  95  P.l(s) = 1;
  96  Y.l(s) = 10;
  97   
  98  *       solve the reference case:
  99   
 100  W.fx("labor") = 1;
 101  solve jel using mcp;
 102   
 104   
 105  dl(s,h) = (sum{f, e[f,h]*W[f]} + tshr[h]*sum{s.local, sum{f.local, t[f,s]*W[f]*Y[s]*(delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]}})*alpha[s,h]*sum{s.local, alpha
      [s,h]*P[s]**(sigmac[h]-1)}*P[s]**(-sigmac[h]); display dl;
