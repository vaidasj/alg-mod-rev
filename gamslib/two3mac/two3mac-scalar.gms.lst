  59   
  60  positive
  61  variables
  62          W(f)    factor price,
  63          P(s)    commodity price,
  64          Y(s)    production level;
  65   
  66   
  67   
  68  equations
  69          fmkt(f)         factor market,
  70          cmkt(s)         commodity market,
  71          profit(s)       zero profit;
  72   
  73  *       Factor supply (endowments) equals factor demand:
  74   
  75  fmkt(f)..       sum(h, e(f,h)) =g= sum(s, (delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]*Y(s));
  76   
  77  *       Commodity output equals commodity demand:
  78   
  79  cmkt(s)..       Y(s) =g= sum(h, (sum{f, e[f,h]*W[f]} + tshr[h]*sum{s.local, sum{f.local, t[f,s]*W[f]*Y[s]*(delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]}})*alpha[s
      ,h]*sum{s.local, alpha[s,h]*P[s]**(sigmac[h]-1)}*P[s]**(-sigmac[h]));
  80   
  81  *       Unit cost equals market price:
  82   
  83  profit(s)..  sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s] =g= P(s);
  84   
  85  parameter DL(s,h);
  86   
  87   
  88  model jel / fmkt.W, cmkt.P, profit.Y/;
  89   
  90  *       compute solution for this dimension problem:
  91   
  92  W.lo(f) = 0.0001;
  93  P.lo(s) = 0.0001;
  94   
  95  W.l(f) = 1;
  96  P.l(s) = 1;
  97  Y.l(s) = 10;
  98   
  99  *       solve the reference case:
 100   
 101  W.fx("labor") = 1;
 102  solve jel using mcp;
 103   
 105   
 106  dl(s,h) = (sum{f, e[f,h]*W[f]} + tshr[h]*sum{s.local, sum{f.local, t[f,s]*W[f]*Y[s]*(delta[f,s]*sum{f.local, delta[f,s]**sigma[s]*(W[f]*(1+t[f,s]))**(1-sigma[s])}**(1/(1-sigma[s]))/phi[s]/(W[f]*(1+t[f,s])))**sigma[s]}})*alpha[s,h]*sum{s.local, alpha
      [s,h]*P[s]**(sigmac[h]-1)}*P[s]**(-sigmac[h]); display dl;
 107   
 108  *       apply tax in test problem:
 109  tshr("rich") = 0.4;
 110  tshr("poor") = 1 - tshr("rich");
