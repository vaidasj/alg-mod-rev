 184   
 185   con(c,s)..    cn(c,s) =e=  epsilon(c,s)*ye + sum((cp,sp), eta(c,s,cp,sp)*p(cp,sp)) ;
 186   
 187   expd(c)..     px(c) =e= - gamma(c)*e(c) + df(c) ;
 188   
 189   supply(c,i).. q(c,i) =e=  z(i) + ( p(c,"domestic") - sum(cp, r(cp,i)*p(cp,"domestic")) ) ;
 190   
 191   indc(c,s,i).. x(c,s,i) =e=  z(i) -  ( p(c,s) - sum(sp, alpha(c,sp,i)*p(c,sp)) ) ;
 192   
 193   indcap(i)..   k(i) =e=  z(i) - ( pk(i) - alphal(i)*w - alphak(i)*pk(i) ) ;
 194   
 195   indlab(i)..   li(i) =e=  z(i) - ( w - alphal(i)*w - alphak(i)*pk(i) ) ;
 196   
 197   pric(i)..     sum(c, r(c,i)*p(c,"domestic")) =e= sum((c,sp), sc(c,sp,i)*p(c,sp)) + sk(i)*pk(i) + sl(i)*w ;
 198   
 199   priexp(c)..   p(c,"domestic") =e= px(c) + v(c) + phi ;
 200   
 201   priimp(c)..   p(c,"imported") =e= pm(c) + t(c) + phi ;
 202   
 203   bald(c)..     sum(i, m(c,i)*q(c,i)) =e= sum(i, wi(c,"domestic",i)*x(c,"domestic",i))
 204                                         + wc(c,"domestic")*cn(c,"domestic") + we(c)*e(c) ;
 205   
 206   ballab..   sum(i, wl(i)*li(i)) =e= l ;
 207   
 208   balcap(i).. k(i) =e= kappa(i) ;
 209   
 210   imports..   mt =e= sum(c, nm(c)*( pm(c) + sum(i, wi(c,"imported",i)*x(c,"imported",i))
