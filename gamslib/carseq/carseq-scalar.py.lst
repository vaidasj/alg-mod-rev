  82       defnumCars(c) exactly numCars of class c assigned to positions
  83       defoneCar(p)  one car assigned to each position p
  84       defop(o,p)    option o appears at position p
  85       defopLS(o,p)  option o appears at position p
  86       defviol(o,p)  violations in a block
  87       defviolLS(o,p) violations in a block
  88       defobj        minimize violations;
  89   
  90  defnumCars(c)..     sum(p, cp(c,p)) =e= classData(c,'numCars');
  91   
  92  defoneCar(p)..      sum(c, cp(c,p)) =e= 1;
  93   
  94  defop(o,p)..        sum(c$classData(c,o),cp(c,p)) =l= op(o,p);
  95   
  96   Equation defsumc(o,p);
  97  defsumc(o,p)..      sumc(o,p) =e= sum(c$classData(c,o),cp(c,p));
  98  defopLS(o,p)..      op(o,p) =e= ifthen(sumc(o,p)>=0.5, 1, 0);
  99  *defopLS(o,p)..      op(o,p) =e= ifthen(sum(c$classData(c,o),cp(c,p))>0.5, 1, 0);
