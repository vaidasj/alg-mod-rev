16330  mean(s)    = sum(d, return(s,d))/card(d);
16331  dev(s,d)   = return(s,d)-mean(s);
16332  covar(s,t) = sum(d, dev(s,d)*dev(t,d))/(card(d)-1);
16333  totmean    = sum(s, mean(s))/(card(s));
