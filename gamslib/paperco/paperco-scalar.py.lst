  83   
  84  logbal..     .97*sum(l, logs(l)) =e= sum((w,p), xw(w,p));
  85   
  86  wbal(w,p)..   xw(w,p) =e= ap(w,p)*pulp(p);
  87   
  88  pbal(p)..     sum(q, xp(p,q)) =e= purchase(p) - sales(p) + pulp(p);
  89   
  90  qbal(p,q)..   xp(p,q) =e= aq(p,q)*paper(q);
  91   
  92  obj..  profit =e= sum(p, pp(p)*sales(p))              { sales of pulp }
  93                  + sum(q, pq(q)*paper(q))              { sales of paper }
  94                  - sum(l, plog*logs(l))                { cost of logs }
  95                  - sum((p,q), cp(p,q)*xp(p,q))         { transport cost of pulp }
  96                  - sum((w,p), (cw(w,p)+pc(w))*xw(w,p)) { transport cost of wood }
  97                  - sum(p, pp(p)*purchase(p));
