  83  logbal..     .97*sum(l, logs(l)) =e= sum((w,p), xw(w,p));
  84   
  85  wbal(w,p)..   xw(w,p) =e= ap(w,p)*pulp(p);
  86   
  87  pbal(p)..     sum(q, xp(p,q)) =e= purchase(p) - sales(p) + pulp(p);
  88   
  89  qbal(p,q)..   xp(p,q) =e= aq(p,q)*paper(q);
  90   
  91  obj..  profit =e= sum(p, pp(p)*sales(p))              { sales of pulp }
  92                  + sum(q, pq(q)*paper(q))              { sales of paper }
  93                  - sum(l, plog*logs(l))                { cost of logs }
  94                  - sum((p,q), cp(p,q)*xp(p,q))         { transport cost of pulp }
  95                  - sum((w,p), (cw(w,p)+pc(w))*xw(w,p)) { transport cost of wood }
  96                  - sum(p, pp(p)*purchase(p));
