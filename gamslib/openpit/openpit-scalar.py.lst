  63     openlow(p,s)   set open to zero
  64     brun(p,s,t)    define staircase for B
  65     defpout(p,t)   define pit output
  66     dem(t)         total demand
  67     outlim(p,s,t)  extraction limit
  68     outmax(p,s)    total extraction limit
  69     outall(p,s)    force complete extraction except last one
  70     defobj;
  71   
  72  eone(p,t)..     sum(s, e(p,s,t)) =e= 1;
  73   
  74  etwo(p,t)..     ej(p,t)   =e= sum(s, ord(s)*e(p,s,t));
  75  ethree(p,t-1).. ej(p,t-1) =l= ej(p,t);
  76   
  77  brun(p,s,t)..  b(p,s,t) =e= b(p,s-1,t) - e(p,s-1,t) + e(p,s,t-1) +
  78  (ord(t)=1 and ord(s)=1);
  79   
  80  defpout(p,t).. pout(p,t) =e= sum(s, out(p,s,t));
  81   
  82  dem(t)..  sum(p, pout(p,t)) =e= demand(t);
  83   
  84   
  85  opendef(p,s,t).. open(p,s) =g= b(p,s,t);
  86  openlow(p,s)..   open(p,s) =l= sum(t, b(p,s,t));
  87   
  88  outlim(p,s,t)..  out(p,s,t) =l= evo(p,s)*b(p,s,t);
  89   
  90  outall(p,s).. sum(t, out(p,s,t)) =g= evo(p,s)*open(p,s+1);
  91  outmax(p,s).. sum(t, out(p,s,t)) =l= evo(p,s)*open(p,s);
  92   
  93  defobj.. obj =e= sum((p,s,t), delta(t)*nev(p,s)*out(p,s,t));
  94   
  95  model extract /all /;
  96   
  97  if(card(s)*card(p)*card(t) > 200,
  98     option limrow=0,limcol=0,solprint=off,optcr=0.01
