 439   Parameters rxf(i,j,c)   domestic shipment activity : final products (1000 tpy)
 440              tds          total domestic supply : final products      (1000 tpy)
 441              tif          total import : final product                (1000 tpy)
 442              tl           transportation load                      (1000 ton_km) ;
 443   
 444     rxf(i,j,cf) = xf.l(cf,i,j) ;
 445             tds = sum((cf,i,j), xf.l(cf,i,j));
 446             tif = sum((cf,j), vf.l(cf,j));
 447     tl("rail")  = sum((cs,i,ip), rail(i,ip)*xi.l(cs,i,ip)) ;
 448     tl("road")  = sum((cr,i), vr.l(cr,i)*impd(i,"road"))
 449                 + sum((cf,i,j), xf.l(cf,i,j)*road(j,i))
 450                 + sum((cf,j), vf.l(cf,j)*road(j,"import-pts"));
 451     tl("barge") = sum((cr,i), vr.l(cr,i)*impd(i,"barge")) ;
