   3  *  This the 4th example from Mathematica and LGO. The global solution is at x=0.
   4  *
   5  *  More information at http://www.wolfram.com/products/applications/mathoptimizer/
   6  *
   7  *
   8  *  Mathematica, MathOptimizer - An Advanced Modeling and Optimization System
   9  *  for Mathematica Users, http://www.wolfram.com/products/applications/mathoptimizer/
  10  *
  11  *  Janos D Pinter, Global Optimization in Action, Kluwer Academic Publishers,
  12  *  Dordrecht/Boston/London, 1996.
  13  *
  14  *  Janos D Pinter, Computational Global Optimization in Nonlinear Systems,
  15  *  Lionheart Publishing, Inc., Atlanta, GA, 2001
  16  *
  18   
  19  variables x1, x2, obj; equations objdef, eq1, ineq1;
  20   
  21  objdef.. obj =e= sqr(2*sqr(x1)-sqr(x2)) + sqr(x2 - 6*sqr(x1));
  22   
  23  eq1..    x1  =e= 10*x2 + 100*Sin[2*x1+3*x2];
  24   
  25  ineq1..  x2 + x1 =l= 2;
  26   
  27  model m /all/;
  28   
  29  x1.lo=-10; x2.lo=-10; x1.up=+10; x2.up=+10;
  30   
  31  set row / one,two,three, global /; parameter report(row,*) summary solution report;
  32   
  33  x1.l = -4; x2.l = -2; // leads to local solution
  34  report('one','x1_0') = x1.l;
  35  report('one','x2_0') = x2.l;
  36  solve m us nlp min obj;
  37  report('one','x1.l') = x1.l;
  38  report('one','x2.l') = x2.l;
  39  report('one','modelstat') = m.modelstat;
  40   
  41   
  42  x1.l = -2; x2.l = -1; // leads to local optimum
  43  report('two','x1_0') = x1.l;
  44  report('two','x2_0') = x2.l;
  45  solve m us nlp min obj;
  46  report('two','x1.l') = x1.l;
  47  report('two','x2.l') = x2.l;
  48  report('two','modelstat') = m.modelstat;
  49   
  50  x1.l =  1; x2.l = -1; // leads to global optimum
  51  report('three','x1_0') = x1.l;
  52  report('three','x2_0') = x2.l;
  53  solve m us nlp min obj;
  54  report('three','x1.l') = x1.l;
  55  report('three','x2.l') = x2.l;
  56  report('three','modelstat') = m.modelstat;
