  57   
  58  x1.l =  0; x2.l =  0; // is the global optimum
  59  report('global','x1_0') = x1.l;
  60  report('global','x2_0') = x2.l;
  61  solve m us nlp min obj;
  62  report('global','x1.l') = x1.l;
  63  report('global','x2.l') = x2.l;
  64  report('global','modelstat') = m.modelstat;
  65   
  66  acronym global;
  67  report(row,'status')$((abs(report('global','x1_0')-report(row,'x1.l'))
  68                        +abs(report('global','x2_0')-report(row,'x2.l'))) < 1e-6) = global;
  69   
  70  display report;


