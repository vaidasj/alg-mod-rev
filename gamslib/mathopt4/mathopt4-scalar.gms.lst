C o m p i l a t i o n


  58   
  59  x1.l =  0; x2.l =  0; // is the global optimum
  60  report('global','x1_0') = x1.l;
  61  report('global','x2_0') = x2.l;
  62  solve m us nlp min obj;
  63  report('global','x1.l') = x1.l;
  64  report('global','x2.l') = x2.l;
  65  report('global','modelstat') = m.modelstat;
  66   
  67  acronym global;
  68  report(row,'status')$((abs(report('global','x1_0')-report(row,'x1.l'))
  69                        +abs(report('global','x2_0')-report(row,'x2.l'))) < 1e-6) = global;
  70   
  71  display report;


