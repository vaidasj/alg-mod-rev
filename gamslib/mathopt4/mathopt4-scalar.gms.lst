   4  *  This the 4th example from Mathematica and LGO. The global solution is at x=0.
   5  *
   6  *  More information at http://www.wolfram.com/products/applications/mathoptimizer/
   7  *
   8  *
   9  *  Mathematica, MathOptimizer - An Advanced Modeling and Optimization System
  10  *  for Mathematica Users, http://www.wolfram.com/products/applications/mathoptimizer/
  11  *
  12  *  Janos D Pinter, Global Optimization in Action, Kluwer Academic Publishers,
  13  *  Dordrecht/Boston/London, 1996.
  14  *
  15  *  Janos D Pinter, Computational Global Optimization in Nonlinear Systems,
  16  *  Lionheart Publishing, Inc., Atlanta, GA, 2001
  17  *
  19   
  20  variables x1, x2, obj; equations objdef, eq1, ineq1;
  21   
  22  objdef.. obj =e= sqr(2*sqr(x1)-sqr(x2)) + sqr(x2 - 6*sqr(x1));
  23   
  24  eq1..    x1  =e= 10*x2 + 100*Sin[2*x1+3*x2];
  25   
  26  ineq1..  x2 + x1 =l= 2;
  27   
  28  model m /all/;
  29   
  30  x1.lo=-10; x2.lo=-10; x1.up=+10; x2.up=+10;
  31   
  32  set row / one,two,three, global /; parameter report(row,*) summary solution report;
  33   
  34  x1.l = -4; x2.l = -2; // leads to local solution
  35  report('one','x1_0') = x1.l;
  36  report('one','x2_0') = x2.l;
  37  solve m us nlp min obj;
  38  report('one','x1.l') = x1.l;
  39  report('one','x2.l') = x2.l;
  40  report('one','modelstat') = m.modelstat;
  41   
  42   
  43  x1.l = -2; x2.l = -1; // leads to local optimum
  44  report('two','x1_0') = x1.l;
  45  report('two','x2_0') = x2.l;
  46  solve m us nlp min obj;
  47  report('two','x1.l') = x1.l;
  48  report('two','x2.l') = x2.l;
  49  report('two','modelstat') = m.modelstat;
  50   
  51  x1.l =  1; x2.l = -1; // leads to global optimum
  52  report('three','x1_0') = x1.l;
  53  report('three','x2_0') = x2.l;
  54  solve m us nlp min obj;
  55  report('three','x1.l') = x1.l;
  56  report('three','x2.l') = x2.l;
  57  report('three','modelstat') = m.modelstat;
