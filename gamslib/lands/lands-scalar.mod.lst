C o m p i l a t i o n


  58   
  59  model det / all /;
  60   
  61   
  62   
  63  set s nodes     / s-1*s-3 /
  64  parameter dvar(s) / s-1 3,  s-2 5,  s-3 7 /
  65            prob(s) / s-1 .3, s-2 .4, s-3 .3 /
  66   
  67  Parameter repdet Scenario report;
  68   
  69  loop(s,
  70     d('mode-1') = dvar(s);
  71     solve det minimizing cost using lp;
  72     repdet('cost',s) = cost.l;
  73     repdet(i,s)      = x.l(i);
  74     repdet('prob',s) = prob(s);
  75     det.solprint=2 );
  76   
  77  *** make model stochastic
  78   
  79  parameter ds(j,s) stochastic demand;
  80   
  81  Positive Variable ys(i,j,s)  operating level
  82   
  83  Equations defcosts    definition of total cost
  84            powbals(i,s)  power balance
  85            dembals(j,s)  demand balance;
  86   
  87  defcosts.. cost =e= sum(i, c(i)*x(i))
  88                    + sum((i,j,s), prob(s)*f(i,j)*ys(i,j,s));
  89   
  90  powbals(i,s).. sum(j, ys(i,j,s)) =l= x(i);
  91   
  92  dembals(j,s).. sum(i, ys(i,j,s)) =g= ds(j,s);
  93   
  94  model stoc / defcosts,mincap,bbal,powbals,dembals /;
  95   
  96  ds(j,s)        = d(j);
  97  ds('mode-1',s) = dvar(s);
  98   
  99  solve stoc minimizing cost using lp;
 100  repdet('cost','hedge') = cost.l;
 101  repdet(i,'hedge')      = x.l(i);
 102   
 103  display repdet;
