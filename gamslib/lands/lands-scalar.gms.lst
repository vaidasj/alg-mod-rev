  59   
  60  model det / all /;
  61   
  62   
  63   
  64  set s nodes     / s-1*s-3 /
  65  parameter dvar(s) / s-1 3,  s-2 5,  s-3 7 /
  66            prob(s) / s-1 .3, s-2 .4, s-3 .3 /
  67   
  68  Parameter repdet Scenario report;
  69   
  70  loop(s,
  71     d('mode-1') = dvar(s);
  72     solve det minimizing cost using lp;
  73     repdet('cost',s) = cost.l;
  74     repdet(i,s)      = x.l(i);
  75     repdet('prob',s) = prob(s);
  76     det.solprint=2 );
  77   
  78  *** make model stochastic
  79   
  80  parameter ds(j,s) stochastic demand;
  81   
  82  Positive Variable ys(i,j,s)  operating level
  83   
  84  Equations defcosts    definition of total cost
  85            powbals(i,s)  power balance
  86            dembals(j,s)  demand balance;
  87   
  88  defcosts.. cost =e= sum(i, c(i)*x(i))
  89                    + sum((i,j,s), prob(s)*f(i,j)*ys(i,j,s));
  90   
  91  powbals(i,s).. sum(j, ys(i,j,s)) =l= x(i);
  92   
  93  dembals(j,s).. sum(i, ys(i,j,s)) =g= ds(j,s);
  94   
  95  model stoc / defcosts,mincap,bbal,powbals,dembals /;
  96   
  97  ds(j,s)        = d(j);
  98  ds('mode-1',s) = dvar(s);
  99   
 100  solve stoc minimizing cost using lp;
 101  repdet('cost','hedge') = cost.l;
 102  repdet(i,'hedge')      = x.l(i);
 103   
 104  display repdet;
