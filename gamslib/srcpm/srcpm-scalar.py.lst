 177   Parameters  dema(cf)   demand parameter a
 178               demb(cf)   demand parameter b ;
 179   
 180    demb(cf) = 1/ddat(cf,"elas") + 1;
 181    dema(cf) = ddat(cf,"ref-p")/demb(cf)/ddat(cf,"ref-q")**(demb(cf)-1);
 182    Display dema,demb,ddat;
 183   
 184   
 185   Variables  z(p,l)     process level                     (mbd)
 186              u(c)       supply of crude                   (mbd)
 187              w(pt)      transfer activities               (mbd)
 188              xf(c)      shipment of final products        (mbd)
 189              pcost      production cost                   (mill $ per day)
 190              scps       consumers and producers surplus   (mill $ per day)
 191              positive variables z, u, w, xf;
 192   
 193   Equations  mb(c)      material balance                  (mbd)
 194              cc(m,l)    capacity constraint               (mbd)
 195              ccr(l)     capacity restriction - heavy ends (mbd)
 196              pcdef      production cost definition        (mill $ per day)
 197              dcps       definition of surplus             (mill $ per day) ;
 198   
 199   mb(c).. mcpe(c) +sum((p,l), a(c,l,p)*z(p,l)) + u(c)$cr(c) + sum(pt, tr(c,pt)*w(pt)) =g= xf(c)$cf(c);
 200   
 201   cc(m,l)..  sum(p, b(m,p)*z(p,l)) =l= k(m,l);
 202   
 203   ccr(l)..   sum(p, hecr(l,p)*z(p,l)) =l= hecc(l);
 204   
 205   pcdef..  pcost =e= sum((p,l), pc(l,p)*z(p,l)) + sum(pt, trc(pt)*w(pt)) + sum(cr, crd(cr,"price")*u(cr));
 206   
 207   dcps..   scps  =e= sum(cf, dema(cf)*xf(cf)**demb(cf) - ddat(cf,"ref-t")*xf(cf)) - pcost ;
 208   
 209   u.up(cr) = crd(cr,"limit");   w.up(pt) = trb(pt);
 210   
 211   Model refnlp world ref model nlp / mb,cc,ccr,pcdef,dcps /
 212         reflp  world ref model lp  / mb,cc,ccr,pcdef / ;
 213   
 214   xf.fx(cf) = ddat(cf,"ref-q");
 215   Solve reflp minimizing pcost using lp ;
 216   
 217   xf.lo(cf) = 2; xf.up(cf) = +inf;
 218   Solve refnlp maximizing scps using nlp ;
