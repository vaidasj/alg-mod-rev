 213              midyear(tg)    / 1979-81 = 1980, 1982-84 = 1983, 1985-87 = 1986 / ;
 214   
 215    cn75(j,cq)       = sum(c75, alpha(c75,cq)*cf75(j,c75));
 216    cn75t(cq)        = sum(j, cn75(j,cq));
 217    rdc(j,cq)        = cn75(j,cq)/cn75t(cq);
 218    cinc(cq)         = (dem80("medium",cq) - cn75t(cq))/5;
 219    db(cq,j,tg)      = rdc(j,cq)*(cn75t(cq) + cinc(cq)*(midyear(tg)-1975));
