 227  *[domestic production] ----
 228  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 229  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 230  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 231  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 232  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 233   
 234  *[government behavior] ----
 235  eqTd..          Td      =e= taud*sum(h, pf(h)*FF(h));
 236  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 237  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 238  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 239                              -Sg)/pq(i);
 240  *[investment behavior] ----
 241  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 242   
 243  *[savings] ----------------
 244  eqSp..          Sp      =e= ssp*sum(h, pf(h)*FF(h));
 245  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 246   
 247  *[household consumption] --
 248  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td)
 249                              /pq(i);
 250  *[international trade] ----
 251  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 252  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 253  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 254                          =e= sum(i, pWm(i)*M(i));
 255   
 256  *[Armington function] -----
 257  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 258                              *D(i)**eta(i))**(1/eta(i));
 259  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 260                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 261  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 262                              **(1/(1-eta(i)))*Q(i);
 263   
 264  *[transformation function] -----
 265  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 266                              *D(i)**phi(i))**(1/phi(i));
 267  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 268                              /pe(i))**(1/(1-phi(i)))*Z(i);
 269  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 270                              /pd(i))**(1/(1-phi(i)))*Z(i);
