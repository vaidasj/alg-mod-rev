 141   
 142  deltam(i)=(1+taum(i))*M0(i)**(1-eta(i))
 143            /((1+taum(i))*M0(i)**(1-eta(i)) +D0(i)**(1-eta(i)));
 144  deltad(i)=D0(i)**(1-eta(i))
 145            /((1+taum(i))*M0(i)**(1-eta(i)) +D0(i)**(1-eta(i)));
 146  gamma(i)=Q0(i)/(deltam(i)*M0(i)**eta(i)+deltad(i)*D0(i)**eta(i))
 147           **(1/eta(i));
 148   
 149  xie(i)=E0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 150  xid(i)=D0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 151  theta(i)=Z0(i)
 152           /(xie(i)*E0(i)**phi(i)+xid(i)*D0(i)**phi(i))**(1/phi(i));
