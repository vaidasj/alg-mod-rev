 225  ;
 226  *[domestic production] ----
 227  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 228  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 229  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 230  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 231  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 232   
 233  *[government behavior] ----
 234  eqTd..          Td      =e= taud*sum(h, pf(h)*FF(h));
 235  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 236  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 237  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 238                              -Sg)/pq(i);
 239  *[investment behavior] ----
 240  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 241   
 242  *[savings] ----------------
 243  eqSp..          Sp      =e= ssp*sum(h, pf(h)*FF(h));
 244  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 245   
 246  *[household consumption] --
 247  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td)
 248                              /pq(i);
 249  *[international trade] ----
 250  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 251  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 252  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 253                          =e= sum(i, pWm(i)*M(i));
 254   
 255  *[Armington function] -----
 256  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 257                              *D(i)**eta(i))**(1/eta(i));
 258  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 259                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 260  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 261                              **(1/(1-eta(i)))*Q(i);
 262   
 263  *[transformation function] -----
 264  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 265                              *D(i)**phi(i))**(1/phi(i));
 266  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 267                              /pe(i))**(1/(1-phi(i)))*Z(i);
 268  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 269                              /pd(i))**(1/(1-phi(i)))*Z(i);
