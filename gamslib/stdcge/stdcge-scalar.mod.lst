 225  *[domestic production] ----
 226  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 227  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 228  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 229  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 230  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 231   
 232  *[government behavior] ----
 233  eqTd..          Td      =e= taud*sum(h, pf(h)*FF(h));
 234  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 235  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 236  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 237                              -Sg)/pq(i);
 238  *[investment behavior] ----
 239  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 240   
 241  *[savings] ----------------
 242  eqSp..          Sp      =e= ssp*sum(h, pf(h)*FF(h));
 243  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 244   
 245  *[household consumption] --
 246  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td)
 247                              /pq(i);
 248  *[international trade] ----
 249  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 250  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 251  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 252                          =e= sum(i, pWm(i)*M(i));
 253   
 254  *[Armington function] -----
 255  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 256                              *D(i)**eta(i))**(1/eta(i));
 257  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 258                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 259  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 260                              **(1/(1-eta(i)))*Q(i);
 261   
 262  *[transformation function] -----
 263  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 264                              *D(i)**phi(i))**(1/phi(i));
 265  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 266                              /pe(i))**(1/(1-phi(i)))*Z(i);
 267  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 268                              /pd(i))**(1/(1-phi(i)))*Z(i);
