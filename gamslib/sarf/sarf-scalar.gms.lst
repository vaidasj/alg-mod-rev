crop data


 383   
 384   Scalar    rho         interest rate                                    /     .04 /
 385             land        farm size                                  (ha)  / 1600    /
 386             lcost       labor cost             (1000 rials per man-day)  /     .135 /
 387             watercost   water cost (rials per 1000 cubic meters per ha)  /     .267 /
 388             hrtoday     hours in a man-day                               /    6     /
 389   
 390   Parameter oc(g,mn,mn) operating costs                                       (1000 rials per ha)
 391             avail(mn)   equipment avaialbility               (hours of utilization per fortnight)
 392             life(mn)    expected life of equipment                                        (years)
 393             crf(mn)     capital recovery factor
 394             cap(mn)     amortized capital cost                                       (1000 rials)
 395             pmisc(c)      cost of inputs - seed pesticide herbicide & fertilizer (1000 rials per ha)
 396             pcrop(c)      price of agricultural commodities                     (1000 rials per ton)
 397             yield(c,w)    crop yields along yield-water curve                   (metric tons per ha)
 398             water(c,w)    water requirements along yield-water curve      (1000 cubic meters per ha)
 399             length(c,s)   length of cropping season for crop under schedule             (fortnights)
 400             luse(c,t,s)   land use possibilities
 401             treq(g,t,c,s) agricultural tasks requirements
 402             agrol(c)      agronomic constraints                                                 (ha);
 403   
 404   oc(g,m,n) = (equip(m,"opcost") + equip(n,"opcost"))*tech(g,m,n)/1000;
 405   avail(mn)  = equip(mn,"avail");
 406   life(mn)   = equip(mn,"life");
 407   
 408   pmisc(c)   = cropdata("seed",c) + cropdata("fertilize",c) + cropdata("pesticide",c) + cropdata("herbicide",c);
 409   pcrop(c)   = cropdata("price",c);
 410   yield(c,w) = cropdata("yield",c)*stress("yield",w,c);
 411   water(c,w) = cropdata("water",c)*stress("water",w,c);
 412   agrol(c)   = land*cropdata("crop-limit",c);
 413   
 414   crf(mn)$equip(mn,"life") = rho*(1 + rho)**equip(mn,"life")/((1 + rho)**equip(mn,"life") - 1);
 415   cap(mn) = crf(mn)*equip(mn,"cost");
 416   
 417   length(c,s)$cs(c,"start",s) = mod(card(t) + cs(c,"end",s) - cs(c,"start",s),card(t)) + 1;
 418   luse(c,t++(cs(c,"start",s)-1),s) = 1$(ord(t) le length(c,s));
 419   
 420   treq(g,t,c,s)  = 1$(atask(c,g,s) eq ord(t)) + btask(c,g,s,t);
 421   treq("transport",t,c,s) = treq("transport",t,c,s)*cropdata("trans-adj",c)*yield(c,"normal");
 422   
 423   cposs(c,s)       = yes$length(c,s);
 424   taskposs(g,t)    = sum((c,s), yes$treq(g,t,c,s));
 425   
 426  * special adjustments for cotton picking:
 427   
 428   Parameter tadj(g)     extra tasks required for mechanical cotton picking (units per ha) / spray    1 /
 429             loss(c)     product loss from mechanical cotton picking;
 430   
 431   loss("cotton")      = .2*yield("cotton","normal");
 432   taskposs("spray",t) = taskposs("spray",t) + taskposs("harvest-c",t);
