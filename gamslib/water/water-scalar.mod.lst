  98  *
  99   
 100   d.lo(a)  = dmin;              d.up(a)  = dmax;
 101   h.lo(rn) = node(rn,"height"); h.lo(dn) = node(dn,"height") + 7.5 + 5.0*node(dn,"demand");
 102   s.lo(rn) = 0;                 s.up(rn) = node(rn,"supply");
 103   
