 267  eqTd(r)..       Td(r)   =e= taud(r)*sum(h, pf(h,r)*FF(h,r));
 268  eqTz(i,r)..     Tz(i,r) =e= tauz(i,r)*pz(i,r)*Z(i,r);
 269  eqTm(i,r)..     Tm(i,r) =e= taum(i,r)*pm(i,r)*M(i,r);
