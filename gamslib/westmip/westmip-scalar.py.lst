 158   
 159   mb(t,i)..  sum(j, a(i,j)*x(t,j)) + m(t,i)$im(i) =e= e(t,i)$ie(i) + alpha(i)*c(t) + cbb(i);
 160   
 161   cc(t,i,n)$in(i,n).. x(t,i) =l= k(t,i,n) ;
 162   
 163   ecc(i,n)$in(i,n)..  ke3(i,n) + ke8(i,n) =e= k("terminal",i,n) - x("terminal",i);
 164   
 165   cb(te-1,i,n)$in(i,n).. k(te,i,n) =e= k(te-1,i,n) + v(te-1,i,n);
 166   
 167   bi(te+1,i,n)$es(i,n).. v(te,i,n) =l= vmax*f(te,i,n);
 168   
 169   rb(te,r)..  sum(i, u(i,r)*x(te,i) + (m(te,i)$im(i) - e(te,i)$ie(i))$forn(r) )$t(te)
 170             + sum((i,n)$in(i,n), inv(i,n,"fixed",r)*f(te,i,n) + inv(i,n,"prop",r)*v(te,i,n) )$tb(te)
 171             + (1+rho)*d(te-1) - d(te)$tb(te) + ms(te)$forn(r) - .8*ms(te)$dom(r)
 172             + (ufe(te)$tb(te) - ufe(te-1))$forn(r) + ar(r)*vr$tl(te) =l= rbase(r)$ti(te);
 173   
 174   vc38(r)..   sum((i,n)$in(i,n), aic(i,n,r)*(ke3(i,n) + ke8(i,n)) ) =g= ar(r)*(vc3 + vc8);
 175   
 176   clow(t+1).. c(t+1) =g= cmin + c(t);
 177   
 178   xlow(t+1).. x(t+1,"primary") =g= x(t,"primary");
 179   
 180   vcdef(i)..  sum(j, a(i,j)*(x("terminal",j) - kb(j,"normal")) ) =g= alpha(i)*vc;
 181   
 182   term..      wterm =e= dft1*vr + dft3*vc3 + dft8*vc8 + dft1*vc ;
 183   
 184   obj..       welfare =e= sum(t, delt(t)*c(t)) + wterm;
