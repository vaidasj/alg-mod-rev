 233  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i));
 234   
 235  *[government behavior] ----
 236  eqTd..          Td      =e= taud*sum(h, pf(h)*FF(h));
 237  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 238  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 239  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 240                              -Sg)/pq(i);
 241   
 242  *[investment behavior] ----
 243  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 244   
 245  *[savings] ----------------
 246  eqSp..          Sp      =e= ssp*sum(h, pf(h)*FF(h));
 247  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 248   
 249  *[household consumption] --
 250  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h)) -Sp -Td)
 251                              /pq(i);
 252   
 253  *[international trade] ----
 254  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 255  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 256  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf =e= sum(i, pWm(i)*M(i));
 257  eqfe(i)..       E(i)/E0(i) =e= (pWe(i)/pWe0(i))**(-sigma(i));
 258  eqfm(i)..       M(i)/M0(i) =e= (pWm(i)/pWm0(i))**psi(i);
