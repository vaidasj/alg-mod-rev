 136  ax(i,j) =X0(i,j)/Z0(j);
 137  ay(j)   =Y0(j)/Z0(j);
 138  mu(i)   =Xg0(i)/sum(j, Xg0(j));
 139  lambda(i)=Xv0(i)/(Sp0+Sg0+Sf);
 140   
 141  deltam(i)=(1+taum(i))*M0(i)**(1-eta(i))
 142          /((1+taum(i))*M0(i)**(1-eta(i)) +D0(i)**(1-eta(i)));
 143  deltad(i)=D0(i)**(1-eta(i))
 144          /((1+taum(i))*M0(i)**(1-eta(i)) +D0(i)**(1-eta(i)));
 145  gamma(i)=Q0(i)/(deltam(i)*M0(i)**eta(i)+deltad(i)*D0(i)**eta(i))
 146          **(1/eta(i));
 147   
 148  xie(i)=E0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 149  xid(i)=D0(i)**(1-phi(i))/(E0(i)**(1-phi(i))+D0(i)**(1-phi(i)));
 150  theta(i)=Z0(i)/(xie(i)*E0(i)**phi(i)+xid(i)*D0(i)**phi(i))**(1/phi(i));
