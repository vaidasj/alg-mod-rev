  81  eqF(h,j)..      F(h,j)  =e= beta(h,j)*pz(j)*Z(j)/pf(h);
  82  eqpx(i)..       X(i)    =e= Z(i);
  83  eqpf(h)..       sum(j, F(h,j)) =e= FF(h);
  84  eqZ(i)..        px(i)   =e= pz(i);
