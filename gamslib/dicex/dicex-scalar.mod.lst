  83  parameter rep1 Chance of winning against next;
  84  parameter chk(vals);
  85  parameter fv(d,f) 'computed face values';
  86   
  87  if( dice2.modelstat = 1 or dice2.modelstat = 8,
  88    rep1(d,f) = 100*sum(fp, comp.l(d,f,fp)) / card(f);
  89    rep1(d,'chance') = sum(f, rep1(d,f))/card(f);
  90    option rep1:0; display rep1;
  91    fv(d,f) = round(fval.l(d,f));
  92    abort$[card(fv) <> card(vals)] 'inconsistent face values';
  93    chk(vals) = 1;
  94    loop{(d,f)$fv(d,f),
  95      chk(vals)$[ord(vals) eq fv(d,f)] = chk(vals) - 1;
  96    };
  97    if {card(chk), execute_unload 'diceDebug'; };
  98    abort$[card(chk)] 'non-unique face values found', chk, fv;
  99  );


