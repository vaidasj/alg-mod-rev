  58           eq5(d,f)     make face assignment unique;
  59   
  60  eq1(d)     .. sum((f,fp), comp(d,f,fp)) =e= wnx;
  61   
  62  eq2(d,f,fp).. fval(d,f) + big*(1-comp(d,f,fp)) =g= fval(d++1,fp) + 1;
  63   
  64  eq3(d,f-1) .. fval(d,f-1) + 1 =l= fval(d,f);
  65   
  66  eq4(d,f)   .. sum((dp,fp), fnum(dp,fp)*fmap(d,f,dp,fp)) =e= fval(d,f);
  67   
  68  eq5(dp,fp)   .. sum((d,f), fmap(d,f,dp,fp)) =e= 1;
  69   
  70  model dice1 each dice with unique faces / eq1 ,eq2, eq3  /
  71        dice2 all faces are unique        / eq1, eq2, eq3, eq4, eq5 /;
