 247                   + sum((i,cu), fchx*ycu(i,cu) + (vchx/htc)*(qcu(i,cu)/(lmtd(i)+1-ycol(i))))
 248                   + sum((hu,j), fchx*yhu(hu,j) + (vchx/htc)*(qhu(hu,j)/(thu(hu)-tr(j)))))
 249           + beta *( sum((i,cu), costcw*qcu(i,cu))
 250                   + sum((hu,j), costhu(hu)*qhu(hu,j)));
