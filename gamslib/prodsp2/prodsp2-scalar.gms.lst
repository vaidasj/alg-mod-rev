  60  Equations obj       expected cost definition
  61            lbal(j,s) labor balance;
  62   
  63  obj.. EProfit =e=  sum(i, c(i)*x(i)) - 1/card(s)*sum((j,s), q(j)*v(j,s));
  64   
  65  lbal(j,s).. sum(i, t(j,i,s)*x(i)) =l= h(j,s) + v(j,s);
  66   
  67   
  68  model mix universe model / obj, lbal /;
  69   
  70  mix.solprint$(card(s) > 10) = 2;
  71   
  72  solve mix using lp maximizing eprofit;
  73   
  74  display eprofit.l,x.l;
  75   
  76  parameter stage1(*,i) hold the stage 1 results;
  77   
  78  stage1('deterministic',i) = x.l(i);
  79   
  80  *--------------------------------------------------------------------------
  81  * Now the DECIS formulation
  82  *--------------------------------------------------------------------------
  83   
  84  Equations
  85    obj2      cost function
  86    lbal2(j)  labor balance
  87    extra     prevent unbounded relaxed master problem
  88  ;
  89   
  90  variables
  91    profit    to be maximized
  92    v2(j)     labor purchased
  93  ;
  94  positive variable v2;
  95   
  96  parameter t2(j,i) mean values;
  97  t2(j,i) = (trand(j,'min',i) + trand(j,'max',i))/2;
  98   
  99  parameter h2(j) mean values / work-1 6000, work-2 4000 /;
 100   
 101   
 102  obj2.. profit =e= sum(i, c(i)*x(i)) - sum(j, q(j)*v2(j));
 103  lbal2(j).. sum(i, t2(j,i)*x(i)) =l= h2(j) + v2(j);
 104   
 105  extra.. sum(i, c(i)*x(i)) =l= 1000000;
 106   
 107  model mix2 decis model /obj2,lbal2, extra/;
