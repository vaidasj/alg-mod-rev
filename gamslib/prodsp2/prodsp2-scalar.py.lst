  61  Equations obj       expected cost definition
  62            lbal(j,s) labor balance;
  63   
  64  obj.. EProfit =e=  sum(i, c(i)*x(i)) - 1/card(s)*sum((j,s), q(j)*v(j,s));
  65   
  66  lbal(j,s).. sum(i, t(j,i,s)*x(i)) =l= h(j,s) + v(j,s);
  67   
  68   
  69  model mix universe model / obj, lbal /;
  70   
  71  mix.solprint$(card(s) > 10) = 2;
  72   
  73  solve mix using lp maximizing eprofit;
  74   
  75  display eprofit.l,x.l;
  76   
  77  parameter stage1(*,i) hold the stage 1 results;
  78   
  79  stage1('deterministic',i) = x.l(i);
  80   
  81  *--------------------------------------------------------------------------
  82  * Now the DECIS formulation
  83  *--------------------------------------------------------------------------
  84   
  85  Equations
  86    obj2      cost function
  87    lbal2(j)  labor balance
  88    extra     prevent unbounded relaxed master problem
  89  ;
  90   
  91  variables
  92    profit    to be maximized
  93    v2(j)     labor purchased
  94  ;
  95  positive variable v2;
  96   
  97  parameter t2(j,i) mean values;
  98  t2(j,i) = (trand(j,'min',i) + trand(j,'max',i))/2;
  99   
 100  parameter h2(j) mean values / work-1 6000, work-2 4000 /;
 101   
 102   
 103  obj2.. profit =e= sum(i, c(i)*x(i)) - sum(j, q(j)*v2(j));
 104  lbal2(j).. sum(i, t2(j,i)*x(i)) =l= h2(j) + v2(j);
 105   
 106  extra.. sum(i, c(i)*x(i)) =l= 1000000;
 107   
 108  model mix2 decis model /obj2,lbal2, extra/;
