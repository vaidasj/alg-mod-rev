  59  Equations obj       expected cost definition
  60            lbal(j,s) labor balance;
  61   
  62  obj.. EProfit =e=  sum(i, c(i)*x(i)) - 1/card(s)*sum((j,s), q(j)*v(j,s));
  63   
  64  lbal(j,s).. sum(i, t(j,i,s)*x(i)) =l= h(j,s) + v(j,s);
  65   
  66   
  67  model mix universe model / obj, lbal /;
  68   
  69  mix.solprint$(card(s) > 10) = 2;
  70   
  71  solve mix using lp maximizing eprofit;
  72   
  73  display eprofit.l,x.l;
  74   
  75  parameter stage1(*,i) hold the stage 1 results;
  76   
  77  stage1('deterministic',i) = x.l(i);
  78   
  79  *--------------------------------------------------------------------------
  80  * Now the DECIS formulation
  81  *--------------------------------------------------------------------------
  82   
  83  Equations
  84    obj2      cost function
  85    lbal2(j)  labor balance
  86    extra     prevent unbounded relaxed master problem
  87  ;
  88   
  89  variables
  90    profit    to be maximized
  91    v2(j)     labor purchased
  92  ;
  93  positive variable v2;
  94   
  95  parameter t2(j,i) mean values;
  96  t2(j,i) = (trand(j,'min',i) + trand(j,'max',i))/2;
  97   
  98  parameter h2(j) mean values / work-1 6000, work-2 4000 /;
  99   
 100   
 101  obj2.. profit =e= sum(i, c(i)*x(i)) - sum(j, q(j)*v2(j));
 102  lbal2(j).. sum(i, t2(j,i)*x(i)) =l= h2(j) + v2(j);
 103   
 104  extra.. sum(i, c(i)*x(i)) =l= 1000000;
 105   
 106  model mix2 decis model /obj2,lbal2, extra/;
