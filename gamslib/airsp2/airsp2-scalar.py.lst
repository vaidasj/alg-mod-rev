 129  *---------------------------------------------------------------------
 130  * Solve expected value problem directly
 131  * expected value of demand is slightly different from
 132  * fixed demand.
 133  *---------------------------------------------------------------------
 134   
 135  parameter expdemand(j) expected value demand;
 136  expdemand(j) =  sum(k, probability(j,k)*stochasticdemand(j,k));
 137  display expdemand, fixeddemand;
 138   
 139  equation expvaldemand(j);
 140   
 141  expvaldemand(j)..  sum(i, pcap(i,j)*x(i,j)) + bumped(j) =g= expdemand(j);
 142   
 143  model expval /cost,avail,expvaldemand/;
 144   
 145  solve expval using lp minimizing z;
 146   
 147  results('Expected value problem',i,j) = x.l(i,j);
 148  obj('Expected value problem') = z.l;
 149  display results;
