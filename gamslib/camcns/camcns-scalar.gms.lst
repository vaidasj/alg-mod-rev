 347   
 348   pmdef(it)..       pm(it) =e= pwm(it)*er*(1 + tm(it)) ;
 349   
 350   pedef(it)..       pe(it)*(1 + te(it)) =e= pwe(it)*er ;
 351   
 352   absorption(i)..   p(i)*x(i) =e= pd(i)*xxd(i) + (pm(i)*m(i))$it(i) ;
 353   
 354   sales(i)..        px(i)*xd(i) =e= pd(i)*xxd(i) + (pe(i)*e(i))$it(i) ;
 355   
 356   actp(i)..         px(i)*(1-itax(i)) =e= pva(i) + sum(j, io(j,i)*p(j) ) ;
 357   
 358   pkdef(i)..        pk(i) =e= sum(j, p(j)*imat(j,i) );
