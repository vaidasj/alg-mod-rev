 231  ;
 232  *[domestic production] ----
 233  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 234  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 235  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 236  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 237  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i))
 238                              +FC(j)/Z(j);
 239  *[government behavior] ----
 240  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 241  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 242  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 243  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 244                              -Sg)/pq(i);
 245  *[investment behavior] ----
 246  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 247  *[savings] ----------------
 248  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 249  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 250  *[household consumption] --
 251  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h))
 252                              +sum(j, FC(j)) -Sp -Td)/pq(i);
 253  *[international trade] ----
 254  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 255  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 256  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 257                          =e= sum(i, pWm(i)*M(i));
 258  *[Armington function] -----
 259  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 260                              *D(i)**eta(i))**(1/eta(i));
 261  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 262                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 263  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 264                              **(1/(1-eta(i)))*Q(i);
 265  *[transformation function] -----
 266  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 267                              *D(i)**phi(i))**(1/phi(i));
 268  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 269                              /pe(i))**(1/(1-phi(i)))*Z(i);
 270  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 271                              /pd(i))**(1/(1-phi(i)))*Z(i);
