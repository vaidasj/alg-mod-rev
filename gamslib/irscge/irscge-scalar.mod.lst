 229  ;
 230  *[domestic production] ----
 231  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 232  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 233  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 234  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 235  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i))
 236                              +FC(j)/Z(j);
 237  *[government behavior] ----
 238  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 239  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 240  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 241  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 242                              -Sg)/pq(i);
 243  *[investment behavior] ----
 244  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 245  *[savings] ----------------
 246  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 247  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 248  *[household consumption] --
 249  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h))
 250                              +sum(j, FC(j)) -Sp -Td)/pq(i);
 251  *[international trade] ----
 252  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 253  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 254  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 255                          =e= sum(i, pWm(i)*M(i));
 256  *[Armington function] -----
 257  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 258                              *D(i)**eta(i))**(1/eta(i));
 259  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 260                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 261  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 262                              **(1/(1-eta(i)))*Q(i);
 263  *[transformation function] -----
 264  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 265                              *D(i)**phi(i))**(1/phi(i));
 266  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 267                              /pe(i))**(1/(1-phi(i)))*Z(i);
 268  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 269                              /pd(i))**(1/(1-phi(i)))*Z(i);
