 230  ;
 231  *[domestic production] ----
 232  eqpy(j)..       Y(j)    =e= b(j)*prod(h, F(h,j)**beta(h,j));
 233  eqX(i,j)..      X(i,j)  =e= ax(i,j)*Z(j);
 234  eqY(j)..        Y(j)    =e= ay(j)*Z(j);
 235  eqF(h,j)..      F(h,j)  =e= beta(h,j)*py(j)*Y(j)/pf(h);
 236  eqpzs(j)..      pz(j)   =e= ay(j)*py(j) +sum(i, ax(i,j)*pq(i))
 237                              +FC(j)/Z(j);
 238  *[government behavior] ----
 239  eqTd..          Td      =e= taud*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 240  eqTz(j)..       Tz(j)   =e= tauz(j)*pz(j)*Z(j);
 241  eqTm(i)..       Tm(i)   =e= taum(i)*pm(i)*M(i);
 242  eqXg(i)..       Xg(i)   =e= mu(i)*(Td +sum(j, Tz(j)) +sum(j, Tm(j))
 243                              -Sg)/pq(i);
 244  *[investment behavior] ----
 245  eqXv(i)..       Xv(i)   =e= lambda(i)*(Sp +Sg +epsilon*Sf)/pq(i);
 246  *[savings] ----------------
 247  eqSp..          Sp      =e= ssp*(sum(h, pf(h)*FF(h)) +sum(j, FC(j)));
 248  eqSg..          Sg      =e= ssg*(Td +sum(j, Tz(j))+sum(j, Tm(j)));
 249  *[household consumption] --
 250  eqXp(i)..       Xp(i)   =e= alpha(i)*(sum(h, pf(h)*FF(h))
 251                              +sum(j, FC(j)) -Sp -Td)/pq(i);
 252  *[international trade] ----
 253  eqpe(i)..       pe(i)   =e= epsilon*pWe(i);
 254  eqpm(i)..       pm(i)   =e= epsilon*pWm(i);
 255  eqepsilon..     sum(i, pWe(i)*E(i)) +Sf
 256                          =e= sum(i, pWm(i)*M(i));
 257  *[Armington function] -----
 258  eqpqs(i)..      Q(i)    =e= gamma(i)*(deltam(i)*M(i)**eta(i)+deltad(i)
 259                              *D(i)**eta(i))**(1/eta(i));
 260  eqM(i)..        M(i)    =e= (gamma(i)**eta(i)*deltam(i)*pq(i)
 261                              /((1+taum(i))*pm(i)))**(1/(1-eta(i)))*Q(i);
 262  eqD(i)..        D(i)    =e= (gamma(i)**eta(i)*deltad(i)*pq(i)/pd(i))
 263                              **(1/(1-eta(i)))*Q(i);
 264  *[transformation function] -----
 265  eqpzd(i)..      Z(i)    =e= theta(i)*(xie(i)*E(i)**phi(i)+xid(i)
 266                              *D(i)**phi(i))**(1/phi(i));
 267  eqE(i)..        E(i)    =e= (theta(i)**phi(i)*xie(i)*(1+tauz(i))*pz(i)
 268                              /pe(i))**(1/(1-phi(i)))*Z(i);
 269  eqDs(i)..       D(i)    =e= (theta(i)**phi(i)*xid(i)*(1+tauz(i))*pz(i)
 270                              /pd(i))**(1/(1-phi(i)))*Z(i);
