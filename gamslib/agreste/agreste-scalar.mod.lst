model definition


 292   ddev(ty).. sum(c, prdev(c,ty)*sales(c)) =e= pdev(ty) - ndev(ty);
 293   
 294   arev..  revenue =e= lprice*xlive + 1000*sum(c, price(c)*sales(c));
 295   
 296   acrop.. cropcost =e= sum((p,s)$ps(p,s), pcost(p)*xcrop(p,s));
 297   
 298   alab..  labcost =e= (fwage*sum(tm, flab(tm)) + twage*sum(tm, tlab(tm)))/dpm + pwage*plab;
 299   
 300   awcc..  cropcost + rationr + vetcost + twage/dpm*sum(tm, tlab(tm)) + pwage*plab =l= wcbar;
 301   
 302   avet..  vetcost =e= vetpr*xlive;
 303   
 304   income..    yfarm =e= revenue + vsc*sum(dr, cons(dr)) - labcost - rationr - vetcost - cropcost
 305               - phi*sum(ty, pdev(ty) + ndev(ty))/card(ty);
 306   
 307   
 308   model agreste farm level model / all /;
 309   
 310   xcrop.up(p,s)$xcropl(p,s) = xcropl(p,s);
 311   flab.up(tm) = famlab;
 312   
 313   solve agreste maximizing yfarm using lp;
 314   
 315   phi = 0;
 316   solve agreste maximizing yfarm using lp;


