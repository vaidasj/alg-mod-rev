model definition


 293   ddev(ty).. sum(c, prdev(c,ty)*sales(c)) =e= pdev(ty) - ndev(ty);
 294   
 295   arev..  revenue =e= lprice*xlive + 1000*sum(c, price(c)*sales(c));
 296   
 297   acrop.. cropcost =e= sum((p,s)$ps(p,s), pcost(p)*xcrop(p,s));
 298   
 299   alab..  labcost =e= (fwage*sum(tm, flab(tm)) + twage*sum(tm, tlab(tm)))/dpm + pwage*plab;
 300   
 301   awcc..  cropcost + rationr + vetcost + twage/dpm*sum(tm, tlab(tm)) + pwage*plab =l= wcbar;
 302   
 303   avet..  vetcost =e= vetpr*xlive;
 304   
 305   income..    yfarm =e= revenue + vsc*sum(dr, cons(dr)) - labcost - rationr - vetcost - cropcost
 306               - phi*sum(ty, pdev(ty) + ndev(ty))/card(ty);
 307   
 308   
 309   model agreste farm level model / all /;
 310   
 311   xcrop.up(p,s)$xcropl(p,s) = xcropl(p,s);
 312   flab.up(tm) = famlab;
 313   
 314   solve agreste maximizing yfarm using lp;
 315   
 316   phi = 0;
 317   solve agreste maximizing yfarm using lp;


