  19  sets i        Assets;
  20  alias(i,j);
  21   
  22  parameters
  23      Q(i,j)    Covariance matrix
  24      alpha(i)  Expected returns
  25      ell(i)    Min Buy-In
  26      u(i)      Max Buy-In;
  27  scalar
  28      rho       Minimum return;
